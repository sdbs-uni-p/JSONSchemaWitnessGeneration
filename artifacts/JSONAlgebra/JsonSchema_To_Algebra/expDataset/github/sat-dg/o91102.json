                    },
                    "locationDescription": {
                        "type": "string"
                    },
                    "longitude": {
                        "type": "string"
                    },
                    "lot": {
                        "type": "string"
                    },
                    "permit": {
                        "type": "string"
                    },
                    "priorpermit": {
                        "type": "string"
                    },
                    "received": {
                        "type": "string"
                    },
                    "schedule": {
                        "type": "string"
                    },
                    "status": {
                        "type": "string"
                    }
                },
                "required": [
                    "objectId",
                    "address",
                    "applicant",
                    "block",
                    "blocklot",
                    "cnn",
                    "facilityType",
                    "fooditems",
                    "latitude",
                    "location",
                    "locationDescription",
                    "longitude",
                    "lot",
                    "permit",
                    "priorpermit",
                    "received",
                    "schedule",
                    "status"
                ]
            }
        }
    },
    "required": [
        "truckDetails"
    ]
