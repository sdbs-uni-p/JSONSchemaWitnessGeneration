{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "array",
    "items": {
        "type": "object",
        "properties": {
            "lastChanged": {
                "type": "integer"
            },
            "isTemporary": {
                "type": "boolean"
            },
            "title": {
                "type": "string"
            },
            "documents": {
                "type": "array",
                "items": {}
            },
            "lastChangedBy": {
                "type": "object",
                "properties": {
                    "email": {
                        "type": "string"
                    },
                    "name": {
                        "type": "string"
                    }
                },
                "required": [
                    "email",
                    "name"
                ]
            },
            "author": {
                "type": "object",
                "properties": {
                    "email": {
                        "type": "string"
                    },
                    "name": {
                        "type": "string"
                    }
                },
                "required": [
                    "email",
                    "name"
                ]
            },
            "id": {
                "type": "string"
            },
            "date": {
                "type": "integer"
            }
        },
        "required": [
            "lastChanged",
            "isTemporary",
            "title",
            "documents",
            "lastChangedBy",
            "author",
            "id",
            "date"
        ]
    }
}