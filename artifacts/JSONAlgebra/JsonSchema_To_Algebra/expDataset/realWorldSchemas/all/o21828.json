{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "description": "Amazon Standard Identification Number",
    "example": "B000J8VLEC",
    "id": "http://schema.hyperrest.com/amazon/asin.json",
    "maxLength": 10,
    "minLength": 10,
    "pattern": "^[A-Z0-9]{10}$",
    "title": "ASIN",
    "type": "string"
}
