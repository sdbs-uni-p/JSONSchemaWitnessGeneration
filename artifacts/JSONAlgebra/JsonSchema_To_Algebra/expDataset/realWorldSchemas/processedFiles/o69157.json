{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Discovered MAC address data",
    "type": "array",
    "items": {
        "type": "array",
        "items": [
            {
                "type": "string",
                "description": "MAC address",
                "pattern": "^(?:[0-9a-fA-F]{12}|(?:[0-9a-fA-F]{1,2}[-:]){5}[0-9a-fA-F]{1,2})"
            },
            {
                "type": "string",
                "description": "Port name"
            }
        ],
        "minItems": 2,
        "additionalItems": false
    }
}