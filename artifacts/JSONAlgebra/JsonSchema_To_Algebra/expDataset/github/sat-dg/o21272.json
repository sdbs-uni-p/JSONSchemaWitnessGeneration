                "spotlight",
                "static",
                "tariff",
                "whitehall-admin",
                "whitehall-frontend"
            ],
            "type": "string"
        },
        "route": {
            "additionalProperties": false,
            "properties": {
                "path": {
                    "type": "string"
                },
                "type": {
                    "enum": [
                        "prefix",
                        "exact"
                    ]
                }
            },
            "required": [
                "path",
                "type"
            ],
            "type": "object"
        },
        "routes": {
            "items": {
                "$ref": "#/definitions/route"
            },
            "minItems": 1,
            "type": "array"
        },
        "title": {
            "type": "string"
        },
        "update_type": {
            "enum": [
                "major",
                "minor",
                "republish"
            ]
        },
        "withdrawn_notice": {
            "additionalProperties": false,
            "properties": {
                "explanation": {
                    "type": "string"
                },
                "withdrawn_at": {
                    "format": "date-time"
                }
            },
            "type": "object"
        }
    },
