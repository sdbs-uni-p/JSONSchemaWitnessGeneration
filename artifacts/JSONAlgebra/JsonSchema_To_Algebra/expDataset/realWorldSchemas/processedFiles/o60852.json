{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://raw.githubusercontent.com/mimmi20/ua-result/master/schema/display.json",
    "definitions": {
        "display": {
            "type": "object",
            "properties": {
                "width": {
                    "anyOf": [
                        {
                            "type": "integer",
                            "minimum": 0
                        },
                        {
                            "type": "null"
                        }
                    ]
                },
                "height": {
                    "anyOf": [
                        {
                            "type": "integer",
                            "minimum": 0
                        },
                        {
                            "type": "null"
                        }
                    ]
                },
                "touch": {
                    "anyOf": [
                        {
                            "type": "boolean"
                        },
                        {
                            "type": "null"
                        }
                    ]
                },
                "size": {
                    "anyOf": [
                        {
                            "type": "number",
                            "minimum": 0
                        },
                        {
                            "type": "null"
                        }
                    ]
                }
            },
            "required": [
                "width",
                "height",
                "touch",
                "size"
            ]
        }
    },
    "$ref": "#/definitions/display"
}