{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "array",
    "items": {
        "type": "object",
        "properties": {
            "name": {
                "type": "string"
            },
            "id": {
                "type": "string"
            },
            "team": {
                "type": "string"
            },
            "team_id": {
                "type": "number"
            },
            "rank": {
                "type": "number"
            },
            "date": {
                "type": "string"
            },
            "scores": {
                "type": "array",
                "items": {
                    "type": "object",
                    "properties": {
                        "id": {
                            "type": "string"
                        },
                        "value": {
                            "type": "number"
                        }
                    },
                    "required": [
                        "id",
                        "value"
                    ]
                }
            }
        },
        "required": [
            "name",
            "id",
            "rank",
            "date",
            "scores"
        ]
    },
    "properties": {},
    "additionalProperties": false
}