{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "array",
    "items": {
        "type": "object",
        "properties": {
            "id": {
                "type": "integer",
                "minimum": 1
            },
            "firstName": {
                "type": "string",
                "faker": "name.firstName"
            },
            "lastName": {
                "type": "string",
                "faker": "name.lastName"
            },
            "emailAddress": {
                "type": "string",
                "faker": "internet.email"
            },
            "userName": {
                "type": "string",
                "faker": "internet.userName"
            },
            "createdAt": {
                "type": "string",
                "faker": "date.past"
            },
            "updatedAt": {
                "type": "string",
                "faker": "date.past"
            }
        },
        "required": [
            "id",
            "firstName",
            "lastName",
            "emailAddress",
            "userName",
            "createdAt",
            "updatedAt"
        ]
    }
}