{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "geolocation",
    "description": "GEO Location",
    "type": "object",
    "properties": {
        "longitude": {
            "type": "number"
        },
        "latitude": {
            "type": "number"
        },
        "altitude": {
            "type": "number"
        },
        "accuracy": {
            "type": "number"
        },
        "altitude_accuracy": {
            "type": "number"
        },
        "heading": {
            "type": "number"
        },
        "speed": {
            "type": "number"
        }
    }
}