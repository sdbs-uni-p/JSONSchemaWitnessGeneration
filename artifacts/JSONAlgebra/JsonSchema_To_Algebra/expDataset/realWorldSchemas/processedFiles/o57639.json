{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "O3D3xx spatial bilateral filter configuration",
    "type": "object",
    "properties": {
        "MaskSize": {
            "type": "integer",
            "minimum": 0,
            "maximum": 1,
            "default": 0
        },
        "SigmaPixel": {
            "type": "number",
            "minimum": 0,
            "default": 2,
            "attributes": [
                "private"
            ]
        },
        "SigmaDistance": {
            "type": "number",
            "minimum": 0,
            "default": 0.1,
            "attributes": [
                "private"
            ]
        }
    },
    "required": [
        "MaskSize"
    ]
}