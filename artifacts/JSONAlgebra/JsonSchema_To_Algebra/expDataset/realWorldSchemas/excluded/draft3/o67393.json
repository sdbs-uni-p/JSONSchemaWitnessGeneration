{
    "format": "categories",
    "properties": {
        "Enrollment": {
            "layout": "grid",
            "properties": {
                "endDate": {
                    "default": "31/12/2016",
                    "description": "Enrollment end date",
                    "format": "date",
                    "readonly": true,
                    "title": "End",
                    "type": "string"
                },
                "initialDate": {
                    "default": "01/01/1985",
                    "description": "Enrollment begin date",
                    "format": "date",
                    "readonly": true,
                    "title": "Begin",
                    "type": "string"
                }
            },
            "title": "Enrollment Interval",
            "type": "object"
        },
        "Research and Study Identification": {
            "format": "categories",
            "layout": "grid",
            "properties": {
                "keywords": {
                    "description": "Keywords for this study.",
                    "format": "table",
                    "items": {
                        "properties": {
                            "keyword": {
                                "type": "string"
                            }
                        },
                        "title": "Keyword",
                        "type": "object"
                    },
                    "title": "Study keywords",
                    "type": "array",
                    "uniqueItems": true
                },
                "published": {
                    "format": "checkbox",
                    "title": "Published",
                    "type": "boolean"
                },
                "res_description": {
                    "description": "Description of your main research.",
                    "required": true,
                    "title": "Research Description",
                    "type": "string"
                },
                "research_id": {
                    "description": "Anything that identifies your research among your others researches. (May be a number.)",
                    "required": true,
                    "title": "Research ID",
                    "type": "string"
                },
                "study_description": {
                    "description": "A description of this study.",
                    "required": true,
                    "title": "Study Description",
                    "type": "string"
                },
                "study_id": {
                    "description": "Anything that identifies your study among your others studies for the currrent research.",
                    "required": true,
                    "title": "Study ID",
                    "type": "string"
                },
                "summary": {
                    "description": "Brief summary of this study in the context of your current research.",
                    "format": "textarea",
                    "title": "Study summary",
                    "type": "string"
                }
            },
            "title": "Research and Study Identification",
            "type": "object"
        },
        "cohortSelection": {
            "description": "Selected cohort follows all criterias (AND Logic)",
            "format": "categories",
            "properties": {
                "diagnosisSelection": {
                    "description": "Any of the following diagnosis codes (OR Logic), and none of the exclusions codes.",
                    "format": "table",
                    "items": {
                        "properties": {
                            "ICD0_category": {
                                "default": "ICD10_CATEGORY_PARTIAL",
                                "description": "A partial or full ICD-10 code",
                                "enum": [
                                    "ICD10_CATEGORY_PARTIAL",
                                    "ICD10_CATEGORY_FULL",
                                    "ICD10_SUBCATEGORY",
                                    "ICD10_ETIOLOGY",
                                    "ICD10_EXTENSION"
                                ],
                                "title": "ICD10 Code",
                                "type": "string"
                            },
                            "code": {
                                "maxLength": 8,
                                "minLength": 3,
                                "type": "string"
                            },
                            "exclusion": {
                                "format": "checkbox",
                                "title": "exclusion",
                                "type": "boolean"
                            }
                        },
                        "title": "ICD-10 Diagnosis",
                        "type": "object"
                    },
                    "title": "Diagnosis",
                    "type": "array",
                    "uniqueItems": true
                },
                "surgeriesSelection": {
                    "description": "Filter surgeries by sql pattern like: %REVASC% . Use upper case.",
                    "properties": {
                        "disableSurgeryFilter": {
                            "items": {
                                "enum": [
                                    "Surgery"
                                ],
                                "type": "string"
                            },
                            "title": "Disable filtering by:",
                            "type": "array",
                            "uniqueItems": true
                        },
                        "surgeriesFilter": {
                            "default": "%REVASC%",
                            "required": true,
                            "title": "Surgery filter",
                            "type": "string"
                        }
                    },
                    "title": "Surgeries",
                    "type": "object"
                }
            },
            "title": "Cohort Selection",
            "type": "object"
        },
        "personDemographics": {
            "properties": {
                "disableGenderFilters": {
                    "items": {
                        "enum": [
                            "Gender",
                            "Min. Age",
                            "Max. Age"
                        ],
                        "type": "string"
                    },
                    "title": "Disable filtering by:",
                    "type": "array",
                    "uniqueItems": true
                },
                "gender": {
                    "items": {
                        "enum": [
                            "Male",
                            "Female",
                            "Unknown",
                            "Others"
                        ],
                        "type": "string"
                    },
                    "required": true,
                    "title": "Genders",
                    "type": "array",
                    "uniqueItems": true
                },
                "maxAge": {
                    "default": "110",
                    "description": "Maximum age to participate",
                    "required": true,
                    "title": "Maimum age",
                    "type": "integer"
                },
                "minAge": {
                    "default": "18",
                    "description": "Lowest age to participate on study",
                    "required": true,
                    "title": "Minimum age",
                    "type": "integer"
                }
            },
            "title": "Demographics",
            "type": "object"
        },
        "studyInterval": {
            "layout": "grid",
            "properties": {
                "endDate": {
                    "default": "01/01/2016",
                    "description": "Study end date",
                    "format": "date",
                    "title": "End",
                    "type": "string"
                },
                "initialDate": {
                    "default": "01/01/2010",
                    "description": "Study begin date",
                    "format": "date",
                    "title": "Begin",
                    "type": "string"
                }
            },
            "title": "Study Interval",
            "type": "object"
        }
    },
    "title": " ",
    "type": "object"
}