{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "id": "http://www.ortolang.fr/schemas/1/video#",
    "description": "Schema for metadata extracted from video files",
    "type": "object",
    "additionalProperties": true,
    "properties": {
        "tiff:ImageLength": {
            "type": "string"
        },
        "tiff:ImageWidth": {
            "type": "string"
        },
        "xmpDM:duration": {
            "type": "string"
        },
        "xmpDM:audioSampleRate": {
            "type": "string"
        }
    }
}