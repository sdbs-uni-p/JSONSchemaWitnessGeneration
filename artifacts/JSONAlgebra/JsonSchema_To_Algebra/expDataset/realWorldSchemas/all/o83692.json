{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Location selector",
    "description": "Central American country selection",
    "type": "object",
    "properties": {
        "country": {
            "type": "string",
            "enum": [
                "Belize",
                "Costa Rica",
                "El Salvador",
                "Guatemala",
                "Honduras",
                "Nicaragua",
                "Panama"
            ],
            "default": "Panama"
        }
    }
}