              "id": "http://jsonschema.net/signalk/config/server/version",
              "type": "string",
              "minLength": 1,
              "title": "Version",
              "description": "Current server version",
              "name": "version",
              "default": "0.1"
            },
            "demo": {
              "id": "http://jsonschema.net/signalk/config/server/demo",
              "type": "object",
              "additionalProperties": false,
              "title": "Demo Mode",
              "description": "Demo Mode plays a file as input continuously.",
              "name": "demo",
              "format": "grid",
              "properties": {
                "start": {
                  "id": "http://jsonschema.net/signalk/config/server/demo/start",
                  "type": "boolean",
                  "title": "Start",
                  "description": "Run in demo mode on next startup?",
                  "name": "start",
                  "default": false
                },
                "file": {
                  "id": "http://jsonschema.net/signalk/config/server/demo/file",
                  "type": "string",
                  "minLength": 1,
                  "title": "File",
                  "description": "Demo file, relative to the install dir",
                  "name": "file",
                  "default": "./src/test/resources/samples/ plaka.nmea"
                },
                "delay": {
                  "id": "http://jsonschema.net/signalk/config/server/serial/baud",
                  "type": "integer",
                  "title": "Delay ms",
                  "description": "Delay in millisec between sending messages. Bigger is slower. 0 is as fast as possible",
                  "name": "delay",
                  "default": 250,
                  "minimum": 0
                }
              },
              "required": [
                "start",
                "file",
                "delay"
              ]
            },
