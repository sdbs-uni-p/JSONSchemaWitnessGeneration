{
    "id": "http://schemas.triniti.io/json-schema/gdbots/ncr/mixin/edge/1-0-0.json#",
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "from_ref": {
            "type": "string",
            "pattern": "^[\\w\\/\\.:-]+$",
            "pbj": {
                "type": "identifier",
                "rule": "single"
            }
        },
        "to_ref": {
            "type": "string",
            "pattern": "^[\\w\\/\\.:-]+$",
            "pbj": {
                "type": "identifier",
                "rule": "single"
            }
        },
        "multiplicity": {
            "type": "string",
            "default": "multi",
            "enum": [
                "unknown",
                "multi",
                "simple",
                "many2one",
                "one2many",
                "one2one"
            ],
            "pbj": {
                "type": "string-enum",
                "rule": "single",
                "overridable": true
            }
        },
        "created_at": {
            "type": "string",
            "pattern": "^[1-9]{1}[0-9]{12,15}$",
            "pbj": {
                "type": "microtime",
                "rule": "single"
            }
        }
    },
    "required": [
        "from_ref",
        "to_ref"
    ],
    "additionalProperties": true
}