{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "required": [
        "IdentityPoolId",
        "AllowUnauthenticatedIdentities",
        "IdentityPoolName"
    ],
    "properties": {
        "IdentityPoolId": {
            "type": "string",
            "pattern": "[\\w-]+:[0-9a-f-]+"
        },
        "AllowUnauthenticatedIdentities": {
            "type": "boolean"
        },
        "IdentityPoolName": {
            "type": "string",
            "pattern": "[\\w ]+"
        },
        "DeveloperProviderName": {
            "type": "string",
            "pattern": "[\\w._-]+"
        },
        "OpenIdConnectProviderARNs": {
            "type": "array",
            "items": {
                "type": "string"
            },
            "_uniqueItems": true
        },
        "SupportedLoginProviders": {
            "type": "object",
            "properties": {
                "graph.facebook.com": {
                    "type": "string"
                },
                "accounts.google.com": {
                    "type": "string"
                },
                "www.amazon.com": {
                    "type": "string"
                },
                "api.twitter.com": {
                    "type": "string"
                },
                "www.digits.com": {
                    "type": "string"
                }
            }
        }
    },
    "additionalProperties": false
}