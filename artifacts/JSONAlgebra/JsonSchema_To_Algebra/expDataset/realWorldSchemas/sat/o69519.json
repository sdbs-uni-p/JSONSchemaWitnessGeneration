{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "person": {
            "type": "object",
            "properties": {
                "first_name": {
                    "type": "string",
                    "enum": [
                        "Andrew"
                    ]
                },
                "last_name": {
                    "type": "string",
                    "enum": [
                        "Smith"
                    ]
                }
            },
            "required": [
                "first_name",
                "last_name"
            ],
            "additionalProperties": false
        }
    },
    "required": [
        "person"
    ]
}