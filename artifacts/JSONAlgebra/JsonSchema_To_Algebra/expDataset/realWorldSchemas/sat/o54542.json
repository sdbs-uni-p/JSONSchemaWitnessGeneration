{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "BarChartInPlaceUpdate",
    "description": "The data contained in a BarChart InPlace update object",
    "type": "object",
    "properties": {
        "inplace": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "   position": {
                        "type": "object",
                        "properties": {
                            "x": {
                                "type": "integer",
                                "minimum": 0
                            },
                            "y": {
                                "type": "integer",
                                "minimum": 0
                            }
                        },
                        "required": [
                            "x",
                            "y"
                        ]
                    },
                    "data": {
                        "type": "number"
                    }
                },
                "required": [
                    "position",
                    "data"
                ]
            }
        }
    }
}