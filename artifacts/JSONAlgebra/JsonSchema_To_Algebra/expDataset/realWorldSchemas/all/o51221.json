{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "impact",
    "type": "object",
    "description": "The impact on operations if this construct were to be realized.",
    "properties": {
        "level": {
            "type": "integer",
            "minimum": 0,
            "maximum": 10,
            "description": "The estimated severity of the impact."
        },
        "intended_effects": {
            "type": "array",
            "items": {
                "type": "string",
                "enum": [
                    "Military Advantage",
                    "Economic Advantage",
                    "Political Advantage",
                    "Intellectual Property Theft",
                    "Identify Theft",
                    "Brand Damage",
                    "Degredation of Service",
                    "Denial and Deception",
                    "Destruction",
                    "Disruption",
                    "Exposure",
                    "Extortion",
                    "Fraud",
                    "Harassment",
                    "Watch the World Burn"
                ]
            }
        },
        "description": {
            "type": "string",
            "description": "A prose description of the impact."
        },
        "credibility": {
            "type": "integer",
            "minimum": 0,
            "maximum": 6,
            "description": "The credibility of this statement, using the Admirality scale"
        }
    }
}