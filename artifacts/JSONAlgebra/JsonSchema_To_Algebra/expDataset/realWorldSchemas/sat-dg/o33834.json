{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "description": "Nodes in a Guice injector graph.",
    "javaType": "com.xiantrimble.guice.jsongrapher.InjectorNode",
    "type": "object",
    "required": [
        "id",
        "type"
    ],
    "properties": {
        "id": {
            "type": "string"
        },
        "type": {
            "enum": [
                "implementation",
                "interface"
            ]
        },
        "members": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    }
                }
            }
        },
        "className": {
            "type": "string"
        },
        "annotationName": {
            "type": "string"
        },
        "instanceName": {
            "type": "string"
        },
        "sourceName": {
            "type": "string"
        }
    }
}