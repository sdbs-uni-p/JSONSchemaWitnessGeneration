{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://mcda.drugis.org/survivalDistribution.json#",
    "title": "MCDA survival distribution for the performance table entry of absolute data",
    "type": "object",
    "required": [
        "type",
        "parameters"
    ],
    "additionalProperties": false,
    "properties": {
        "type": {
            "type": "string",
            "enum": [
                "dsurv"
            ]
        },
        "parameters": {
            "type": "object",
            "additionalProperties": false,
            "required": [
                "alpha",
                "beta",
                "summaryMeasure"
            ],
            "properties": {
                "alpha": {
                    "type": "number"
                },
                "beta": {
                    "type": "number"
                },
                "summaryMeasure": {
                    "type": "string",
                    "enum": [
                        "mean",
                        "median",
                        "survivalAtTime"
                    ]
                },
                "time": {
                    "type": "number"
                }
            }
        }
    }
}