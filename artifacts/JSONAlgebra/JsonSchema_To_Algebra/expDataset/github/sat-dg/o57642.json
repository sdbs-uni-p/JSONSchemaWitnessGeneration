{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "O3D3xx temporal exponential filter configuration",
    "type": "object",
    "properties": {
        "MinSmoothDiff": {
            "type": "number",
            "minimum": 0,
            "default": 0.3,
            "attributes": [
                "private"
            ]
        },
        "MinSDAlpha": {
            "type": "number",
            "minimum": 0,
            "maximum": 1,
            "default": 0.2,
            "attributes": [
                "private"
            ]
        },
        "MaxSmoothDiff": {
            "type": "number",
            "minimum": 0,
            "default": 1,
            "attributes": [
                "private"
            ]
        },
        "MaxSDAlpha": {
            "type": "number",
            "minimum": 0,
            "maximum": 1,
            "default": 0.95,
            "attributes": [
                "private"
            ]
        }
    }
}