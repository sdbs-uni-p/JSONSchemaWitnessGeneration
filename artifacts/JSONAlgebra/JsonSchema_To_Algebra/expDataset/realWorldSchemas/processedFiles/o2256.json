{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "id": "http://localhost:9999/schemas/ticket.json",
    "type": "object",
    "properties": {
        "id": {
            "id": "id",
            "type": "integer"
        },
        "message": {
            "id": "message",
            "type": "string"
        },
        "url": {
            "id": "url",
            "type": "string"
        },
        "priority": {
            "id": "priority",
            "enum": [
                "low",
                "medium",
                "high"
            ]
        }
    },
    "required": [
        "id",
        "message",
        "url",
        "priority"
    ]
}