{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Service 002 : DELETE /users/",
    "description": "Delete users listed from a list of ids",
    "type": "array",
    "items": {
        "description": "the list results",
        "type": "object",
        "properties": {
            "id": {
                "type": "integer"
            },
            "status": {
                "type": "string",
                "enum": [
                    "SUCCESS",
                    "ERROR"
                ]
            }
        },
        "required": [
            "id",
            "status"
        ]
    }
}