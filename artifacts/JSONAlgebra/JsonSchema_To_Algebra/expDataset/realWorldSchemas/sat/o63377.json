{
    "$schema": "http://json-schema.org/draft-04/schema",
    "properties": {
        "last_update": {
            "_format": "date-time",
            "type": "string"
        },
        "name": {
            "pattern": "^[^/]+$",
            "type": "string"
        },
        "urls": {
            "format": "urls",
            "type": "object"
        }
    },
    "required": [
        "urls",
        "name",
        "last_update"
    ],
    "type": "object"
}