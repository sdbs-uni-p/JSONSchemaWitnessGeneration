{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "title": "Server ChildDiscussionForm",
    "description": "Server ChildDiscussionForm from Tiamat",
    "type": "object",
    "additionalProperties": false,
    "required": [
        "parentDiscussion"
    ],
    "properties": {
        "parentDiscussion": {
            "type": "object",
            "required": [
                "_id"
            ],
            "properties": {
                "_id": {
                    "type": "string",
                    "pattern": "^[0-9a-fA-F]{24}$"
                }
            }
        }
    }
}