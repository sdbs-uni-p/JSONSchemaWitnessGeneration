{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "pagination_config",
    "properties": {
        "requestElements": {
            "type": "number"
        },
        "keyVisible": {
            "type": "number"
        },
        "requestPage": {
            "type": "number"
        },
        "totalPages": {
            "type": "number"
        },
        "totalElements": {
            "type": "number"
        },
        "isFirstPage": {
            "type": "boolean"
        },
        "isLastPage": {
            "type": "boolean"
        },
        "elementsInPage": {
            "type": "boolean"
        },
        "keysOfPages": {
            "type": "array",
            "items": {
                "type": "number"
            },
            "default": []
        }
    },
    "required": [
        "totalElements",
        "keysVisible",
        "requestElements",
        "requestPage"
    ]
}