{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "events": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "id": {
                        "type": "integer"
                    },
                    "title": {
                        "type": "string"
                    },
                    "url": {
                        "type": "string"
                    },
                    "start": {
                        "type": "string"
                    },
                    "end": {
                        "type": "string"
                    },
                    "address": {
                        "type": "string"
                    },
                    "tags": {
                        "type": "string"
                    }
                },
                "required": [
                    "id",
                    "title",
                    "url",
                    "start"
                ]
            }
        }
    },
    "required": [
        "events"
    ]
}