{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "unixFileOutput",
    "description": "A JSON representation of output from the Unix file program",
    "type": "object",
    "properties": {
        "identifier": {
            "type": "string"
        },
        "version": {
            "type": "string"
        },
        "output": {
            "type": "string"
        },
        "mimeType": {
            "type": "string"
        },
        "magicLocation": {
            "type": "string"
        }
    },
    "required": [
        "identifier",
        "version",
        "output",
        "mimeType",
        "magicLocation"
    ],
    "maxProperties": 5
}