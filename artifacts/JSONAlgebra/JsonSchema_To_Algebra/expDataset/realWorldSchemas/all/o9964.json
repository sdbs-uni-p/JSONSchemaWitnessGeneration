{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "key": {
            "type": "string"
        },
        "payload": {},
        "meta": {},
        "flowVersion": {
            "type": "string",
            "minLength": 1,
            "maxLength": 255
        },
        "deviceId": {
            "type": "string",
            "pattern": "^[A-Fa-f\\d]{24}$"
        }
    },
    "required": [
        "key"
    ],
    "additionalProperties": false
}