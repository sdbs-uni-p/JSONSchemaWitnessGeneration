{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "description": {
            "type": "string"
        },
        "followers": {
            "type": "integer"
        },
        "following": {
            "type": "integer"
        },
        "isExplicit": {
            "type": "boolean"
        },
        "isPrivate": {
            "type": "boolean"
        },
        "isVerified": {
            "type": "boolean"
        },
        "name": {
            "type": "string"
        },
        "totalLoops": {
            "type": "integer",
            "description": "All the views on the videos of the given account."
        },
        "totalLikedPosts": {
            "type": "integer",
            "description": "videos liked by this account"
        },
        "userId": {
            "type": "string"
        },
        "username": {
            "type": "string",
            "description": "the last segment of what vine calls a \"vanity url\""
        },
        "authoredPosts": {
            "type": "integer"
        },
        "posts": {
            "type": "integer"
        }
    },
    "additionalProperties": false,
    "required": [
        "description",
        "followers",
        "following",
        "isExplicit",
        "isPrivate",
        "isVerified",
        "name",
        "totalLoops",
        "totalLikedPosts",
        "userId",
        "username",
        "authoredPosts",
        "posts"
    ]
}