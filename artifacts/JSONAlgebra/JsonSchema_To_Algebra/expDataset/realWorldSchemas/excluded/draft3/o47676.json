{
    "definitions": {
        "contact_detail": {
            "$schema": "http://json-schema.org/draft-03/schema#",
            "description": "A means of contacting an entity",
            "properties": {
                "type": {
                    "description": "A type of medium, e.g. 'fax' or 'email'",
                    "required": true,
                    "type": "string"
                },
                "value": {
                    "description": "A value, e.g. a phone number or email address",
                    "required": true,
                    "type": "string"
                }
            },
            "title": "Contact detail",
            "type": "object"
        },
        "entity": {
            "$schema": "http://json-schema.org/draft-03/schema#",
            "properties": {
                "@type": {
                    "enum": [
                        "Entity"
                    ],
                    "type": "string"
                },
                "birth_date": {
                    "description": "A date of birth",
                    "pattern": "^[0-9]{4}(-[0-9]{2}){0,2}$",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "classification": {
                    "description": "An entity category, e.g. committee",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "contact_details": {
                    "description": "Means of contacting the entity",
                    "items": {
                        "$ref": "#/definitions/contact_detail"
                    },
                    "type": "array"
                },
                "created_at": {
                    "description": "The time at which the resource was created",
                    "format": "date-time",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "death_date": {
                    "description": "A date of death",
                    "pattern": "^[0-9]{4}(-[0-9]{2}){0,2}$",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "description": {
                    "description": "A description of the entity",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "dissolution_date": {
                    "description": "A date of dissolution",
                    "pattern": "^[0-9]{4}(-[0-9]{2}){0,2}$",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "founding_date": {
                    "description": "A date of founding",
                    "pattern": "^[0-9]{4}(-[0-9]{2}){0,2}$",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "identifiers": {
                    "description": "Issued identifiers",
                    "items": {
                        "$ref": "#/definitions/identifier"
                    },
                    "type": "array"
                },
                "links": {
                    "description": "URLs to documents about the entity",
                    "items": {
                        "$ref": "#/definitions/link"
                    },
                    "type": "array"
                },
                "name": {
                    "description": "A primary name, e.g. a legally recognized name",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "other_names": {
                    "description": "Alternate or former names",
                    "items": {
                        "$ref": "#/definitions/other_name"
                    },
                    "type": "array"
                },
                "parent_id": {
                    "description": "The ID of the organization that contains this organization",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "sources": {
                    "description": "URLs to documents from which the resource is derived",
                    "items": {
                        "$ref": "#/definitions/link"
                    },
                    "type": "array"
                },
                "type": {
                    "description": "An entity type, e.g. organization",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "updated_at": {
                    "description": "The time at which the resource was last modified",
                    "format": "date-time",
                    "type": [
                        "string",
                        "null"
                    ]
                }
            },
            "type": "object"
        },
        "identifier": {
            "$schema": "http://json-schema.org/draft-03/schema#",
            "description": "An issued identifier",
            "properties": {
                "identifier": {
                    "description": "An issued identifier, e.g. a DUNS number",
                    "required": true,
                    "type": "string"
                },
                "scheme": {
                    "description": "An identifier scheme, e.g. DUNS",
                    "type": [
                        "string",
                        "null"
                    ]
                }
            },
            "title": "Identifier",
            "type": "object"
        },
        "link": {
            "$schema": "http://json-schema.org/draft-03/schema#",
            "description": "A URL",
            "properties": {
                "note": {
                    "description": "A note, e.g. 'Wikipedia page'",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "url": {
                    "description": "A URL",
                    "format": "uri",
                    "required": true,
                    "type": "string"
                }
            },
            "title": "Link",
            "type": "object"
        },
        "list": {
            "$schema": "http://json-schema.org/draft-03/schema#",
            "properties": {
                "@type": {
                    "enum": [
                        "List"
                    ],
                    "type": "string"
                },
                "created_at": {
                    "description": "The time at which the resource was created",
                    "format": "date-time",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "description": {
                    "description": "A description of the list",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "identifiers": {
                    "description": "Issued identifiers",
                    "items": {
                        "$ref": "#/definitions/identifier"
                    },
                    "type": "array"
                },
                "item_list_order": {
                    "description": "The type of ordering (e.g. ascending, descending, unordered)",
                    "enum": [
                        "ascending",
                        "descending",
                        "unordered"
                    ],
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "links": {
                    "description": "URLs to documents about the list",
                    "items": {
                        "$ref": "#/definitions/link"
                    },
                    "type": "array"
                },
                "name": {
                    "description": "The name of the list",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "number_of_items": {
                    "description": "The number of items in the list",
                    "type": [
                        "integer",
                        "null"
                    ]
                },
                "sources": {
                    "description": "URLs to documents from which the resource is derived",
                    "items": {
                        "$ref": "#/definitions/link"
                    },
                    "type": "array"
                },
                "updated_at": {
                    "description": "The time at which the resource was last modified",
                    "format": "date-time",
                    "type": [
                        "string",
                        "null"
                    ]
                }
            },
            "type": "object"
        },
        "other_name": {
            "$schema": "http://json-schema.org/draft-03/schema#",
            "description": "An alternate or former name",
            "properties": {
                "end_date": {
                    "description": "The date on which the name was abandoned",
                    "pattern": "^[0-9]{4}(-[0-9]{2}){0,2}$",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "name": {
                    "description": "An alternate or former name",
                    "type": "string"
                },
                "note": {
                    "description": "A note, e.g. 'Birth name'",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "start_date": {
                    "description": "The date on which the name was adopted",
                    "pattern": "^[0-9]{4}(-[0-9]{2}){0,2}$",
                    "type": [
                        "string",
                        "null"
                    ]
                }
            },
            "title": "Other name",
            "type": "object"
        },
        "relation": {
            "$schema": "http://json-schema.org/draft-03/schema#",
            "properties": {
                "@type": {
                    "enum": [
                        "Relation"
                    ],
                    "type": "string"
                },
                "created_at": {
                    "description": "The time at which the resource was created",
                    "format": "date-time",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "end_date": {
                    "description": "The date on which the relationship ended",
                    "pattern": "^[0-9]{4}((-[0-9]{2}){0,2}|(-[0-9]{2}){2}(T[0-9]{2}(:[0-9]{2}(:[0-9]{2})?)?Z)?)$",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "identifiers": {
                    "description": "Issued identifiers",
                    "items": {
                        "$ref": "#/definitions/identifier"
                    },
                    "type": "array"
                },
                "links": {
                    "description": "URLs to documents about the list",
                    "items": {
                        "$ref": "#/definitions/link"
                    },
                    "type": "array"
                },
                "name": {
                    "description": "The name of the relation",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "object": {
                    "$ref": "#/definitions/entity",
                    "description": "The object of the relation"
                },
                "sources": {
                    "description": "URLs to documents from which the resource is derived",
                    "items": {
                        "$ref": "#/definitions/link"
                    },
                    "type": "array"
                },
                "start_date": {
                    "description": "The date on which the relationship began",
                    "pattern": "^[0-9]{4}((-[0-9]{2}){0,2}|(-[0-9]{2}){2}(T[0-9]{2}(:[0-9]{2}(:[0-9]{2})?)?Z)?)$",
                    "type": [
                        "string",
                        "null"
                    ]
                },
                "subject": {
                    "description": "The subject of the relation",
                    "items": {
                        "$ref": "#/definitions/entity"
                    },
                    "type": "array"
                },
                "updated_at": {
                    "description": "The time at which the resource was last modified",
                    "format": "date-time",
                    "type": [
                        "string",
                        "null"
                    ]
                }
            },
            "type": "object"
        }
    }
}