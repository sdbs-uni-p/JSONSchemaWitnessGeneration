{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "description": "Parameters for a senpy analysis",
    "type": "object",
    "properties": {
        "name": {
            "type": "string",
            "description": "Name of the parameter"
        },
        "prov:value": {
            "@type": "any",
            "description": "Value of the parameter"
        }
    },
    "required": [
        "name",
        "prov:value"
    ]
}