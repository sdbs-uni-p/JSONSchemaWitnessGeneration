{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "required": [
        "_elements"
    ],
    "properties": {
        "_attributes": {
            "type": "object"
        },
        "_elements": {
            "type": "array",
            "items": {
                "type": "object",
                "required": [
                    "name",
                    "password",
                    "gid",
                    "users"
                ],
                "properties": {
                    "name": {
                        "type": "string",
                        "minLength": 1
                    },
                    "password": {
                        "type": "string"
                    },
                    "gid": {
                        "type": [
                            "integer",
                            "null"
                        ],
                        "minimum": 0
                    },
                    "users": {
                        "type": "array",
                        "items": {
                            "type": "string",
                            "minLength": 1
                        }
                    }
                }
            }
        }
    }
}