{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "name": "string",
    "description": "string",
    "url": "string",
    "icon": "string",
    "background": "string",
    "channels": {
        "type": "array",
        "_uniqueItems": true,
        "items": {
            "type": "object",
            "required": [
                "id"
            ],
            "properties": {
                "id": "string",
                "title": "string",
                "description": "string",
                "dj": "string",
                "djmail": "string",
                "genre": "string",
                "image": "string",
                "largeimage": "string",
                "xlimage": "string",
                "twitter": "string",
                "updated": "string",
                "playlists": {
                    "type": "array",
                    "_uniqueItems": true,
                    "items": {
                        "type": "object",
                        "required": [
                            "url",
                            "format",
                            "quality"
                        ],
                        "properties": {
                            "url": "string",
                            "format": [
                                "mp3",
                                "aac",
                                "aacp"
                            ],
                            "quality": [
                                "low",
                                "high",
                                "highest"
                            ]
                        }
                    }
                },
                "listeners": {
                    "type": "string",
                    "pattern": "[0-9]*"
                },
                "lastPlaying": "string",
                "lastPlayingImage": "url",
                "lastPlayingArtist": "string",
                "lastPlayingAlbum": "string",
                "lastPlayingTrack": "string",
                "lastPlayingLabel": "string",
                "lastPlayingCountry": "string",
                "lastPlayingYear": {
                    "type": "string",
                    "pattern": "[0-9]*"
                }
            }
        }
    }
}