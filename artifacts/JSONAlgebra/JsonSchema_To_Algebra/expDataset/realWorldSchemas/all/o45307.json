{
    "additionalProperties": false,
    "properties": {
        "exclude": {
            "items": {
                "type": "string"
            },
            "type": "array"
        },
        "name": {
            "type": "string"
        },
        "port": {
            "type": "number"
        },
        "readable": {
            "type": "boolean"
        },
        "ssh": {
            "pattern": "^.+@.+$",
            "type": "string"
        },
        "url": {
            "pattern": "^https?://",
            "type": "string"
        },
        "vagrant": {
            "type": "string"
        },
        "writable": {
            "type": "boolean"
        }
    },
    "required": [
        "name",
        "url",
        "readable",
        "writable"
    ],
    "type": "object"
}