{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "additionalProperties": false,
    "type": "object",
    "properties": {
        "keyName": {
            "title": "Storage Key",
            "type": "string",
            "minLength": 1
        },
        "valuePath": {
            "title": "Value",
            "description": "JSON path for getting value to store. If 'Value' is empty, the current value at the provided 'Storage Key' is deleted.",
            "type": "string"
        }
    },
    "required": [
        "keyName",
        "valuePath"
    ]
}