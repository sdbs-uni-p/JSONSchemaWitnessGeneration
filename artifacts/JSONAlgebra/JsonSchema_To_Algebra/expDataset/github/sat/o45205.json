        "properties": {
            "name": {
                "type": "string"
            },
            "rank": {
                "type": "number"
            },
            "date": {
                "type": "string"
            },
            "scores": {
                "type": "array",
                "items": {
                    "type": "object",
                    "properties": {
                        "id": {
                            "type": "string"
                        },
                        "value": {
                            "type": "number"
                        }
                    }
                }
            }
        },
        "required": [
            "name",
            "rank",
            "date",
            "scores"
        ]
    },
    "properties": {},
