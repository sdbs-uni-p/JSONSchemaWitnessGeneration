                "type": "object",
                "properties": {
                    "objectId": {
                        "type": "string"
                    },
                    "address": {
                        "type": "string"
                    },
                    "applicant": {
                        "type": "string"
                    },
                    "block": {
                        "type": "string"
                    },
                    "blocklot": {
                        "type": "string"
                    },
                    "cnn": {
                        "type": "string"
                    },
                    "facilityType": {
                        "type": "string"
                    },
                    "fooditems": {
                        "type": "string"
                    },
                    "latitude": {
                        "type": "string"
                    },
                    "location": {
                        "type": "object",
                        "properties": {
                            "type": {
                                "type": "string"
                            },
                            "coordinates": {
                                "type": "array",
                                "items": {
                                    "type": "integer"
                                }
                            }
                        },
                        "required": [
                            "type",
