    "expires_in": {
        "description": "The acceptable \u201dtime-to-live\u201d in seconds between the merchant\u2019s creation of a payment and the customer\u2019s interaction. This can be set to anything between 5 minutes and 24 hours",
        "type": "integer",
        "minimum": 300,
        "maximum": 86400,
        "exclusiveMinimum": false,
        "exclusiveMaximum": false
    },
    "retries": {
        "description": "The acceptable payment retries for the customers if a transaction failed.",
        "type": "integer",
        "minimum": 1,
        "exclusiveMinimum": false
    },
    "birth_date": {
        "description": "Date of format 'YYYY-MM-DD' years 19xx and 20xx e.g. '1956-12-30'",
        "type": "string",
        "pattern": "^(19|20)\\d{2}-(0|1)\\d-(0|1|2|3)\\d$"
    },
    "risk_params": {
        "description": "set of risk params as described in the Advanced risk management with RiskParams section",
        "type": "object",
        "additionalProperties": false,
        "properties": {
            "ssn": {
                "type": [
                    "string",
                    "number"
                ],
                "minLength": 1,
                "maxLength": 128
            },
            "mac_address": {
                "type": "string",
                "minLength": 1,
                "maxLength": 128
            },
            "session_id": {
                "type": [
                    "string",
                    "number"
                ],
                "minLength": 1,
                "maxLength": 128
            },
            "user_id": {
                "type": [
                    "string",
                    "number"
                ],
                "minLength": 1,
                "maxLength": 128
            },
            "user_level": {
                "type": [
                    "string",
                    "number"
                ],
                "minLength": 1,
                "maxLength": 128
            },
            "email": {
                "type": "string",
                "minLength": 1,
                "maxLength": 128
            },
            "phone": {
                "type": [
                    "string",
                    "number"
                ],
                "minLength": 1,
                "maxLength": 128
            },
            "remote_ip": {
                "type": "string",
                "minLength": 1,
                "maxLength": 128
            },
            "serial_number": {
                "type": [
                    "string",
                    "number"
                ],
                "minLength": 1,
                "maxLength": 128
            },
            "infocapture_token": {
                "type": "string",
                "minLength": 1,
                "maxLength": 128
            },
            "birthday": {
                "extends": "types.json#/birth_date"
            }
        }
    },
    "meta": {
        "description": "A Hashtable when key and value must be strings. Allows to accociate arbitrary data with the parent object.",
        "type": "object",
        "additionalProperties": false,
        "patternProperties": {
            "^[a-zA-Z][a-zA-Z0-9_]+$": {
                "type": "string"
            }
        }
    },
    "risk_params_birthday_only": {
        "description": "risk params with birthday only",
        "type": "object",
        "additionalProperties": false,
        "properties": {
            "birthday": {
                "extends": "types.json#/birth_date"
            }
        }
    },
    "recurring_schedule": {
        "description": "Recurring Schedule",
        "type": "object",
        "$schema": "http://json-schema.org/draft-03/schema",
        "additionalProperties": false,
        "properties": {
            "start_date": {
                "description": "Start date to begin with recurring; format 'YYYY-MM-DD' e.g. '2014-05-24'",
                "required": true,
                "type": "string",
                "pattern": "^20\\d{2}-(0|1)\\d-(0|1|2|3)\\d$"
            },
            "end_date": {
                "description": "End date to stop the recurring schedule; format 'YYYY-MM-DD' e.g. '2017-10-15'",
                "type": "string",
                "pattern": "^20\\d{2}-(0|1)\\d-(0|1|2|3)\\d$"
            },
            "amount": {
                "description": "Amount of the payment in cents, must be > 0",
                "required": true,
                "type": "integer",
                "minimum": 1,
                "exclusiveMinimum": false
            },
            "interval": {
                "description": "recurring interval. must be one of weekly, monthly, anually",
                "required": true,
                "type": "string",
                "enum": [
                    "weekly",
                    "monthly",
                    "anually"
                ]
            },
            "max_retries": {
                "description": "number of times to retry the rebilling before giving up",
                "type": "integer",
                "minimum": 0,
                "maximum": 10,
                "exclusiveMinimum": false,
                "exclusiveMaximum": false
            }
        }
    },
    "mpi_params": {
        "description": "required if an authorize3d or sale3d transaction should be handled synchronous",
        "type": "object",
        "required": true,
        "additionalProperties": false,
        "properties": {
            "cavv": {
                "description": "Verification Id of the authentication. Please note this can be the CAVV for Visa Card or UCAF to identify MasterCard",
                "type": "string",
                "required": true,
                "minLength": 1,
                "maxLength": 255
            },
            "eci": {
                "description": "Electric Commerce Indicator as retured from the MPI.",
                "type": "string",
                "required": true,
                "minLength": 1,
                "maxLength": 255
            },
            "xid": {
                "description": "Transaction ID generated by the 3D Secure service that uniquely identifies a 3D Secure check request",
                "type": "string",
                "required": true,
                "minLength": 1,
                "maxLength": 255
            }
        }
    },
    "transaction_types": {
        "description": "For WpfPayment and MobilePayment. The transaction types that the merchant is willing to accept payments for",
        "type": "array",
        "minItems": 1,
        "items": {
            "enum": [
                "sale",
                "sale3d",
                "authorize",
                "authorize3d",
                "capture",
                "refund",
                "void",
                "init_recurring_sale",
                "recuring_sale",
                "ideal_sale",
                "referenced_fund_transfer",
                "debit_sale",
                "sepa_debit_sale",
                "init_recurring_sepa_debit_sale",
                "init_recurring_sepa_debit_authorize",
                "recurring_sepa_debit_sale",
                "gtd_sepa_debit_sale",
                "init_recurring_gtd_sepa_debit_sale",
                "init_recurring_gtd_sepa_debit_authorize",
                "recurring_gtd_sepa_debit_sale",
                "direct_pay24_sale",
                "giro_pay_sale",
                "pay_safe_card_sale",
                "init_recurring_authorize",
                "purchase_on_account",
                "gtd_purchase_on_account",
                "pay_in_advance",
                "deposit",
                "payment_on_delivery",
                "pay_pal",
                "init_recurring_debit_sale",
                "init_recurring_debit_authorize",
                "recurring_debit_sale",
                "barzahlen_sale",
                "eps_sale"
            ]
        }
    },
    "page": {
        "description": "pagination: the result set page to show",
        "type": "integer",
        "minimum": 1,
        "maximum": 100000,
        "exclusiveMinimum": false,
        "exclusiveMaximum": false
    },
    "per_page": {
        "description": "pagination: the count of entries per page",
        "type": "integer",
        "minimum": 1,
        "maximum": 1000,
        "exclusiveMinimum": false,
        "exclusiveMaximum": false
    },
    "boolean": {
        "enum": [
            true,
            false,
            0,
            1
        ]
    }
}