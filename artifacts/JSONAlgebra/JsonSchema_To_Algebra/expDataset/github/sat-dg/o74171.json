{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "definitions": {
        "login-output": {
            "type": "object",
            "properties": {
                "token": {
                    "type": "string"
                }
            },
            "required": [
                "token"
            ]
        },
        "logout-output": {
            "type": "object",
            "properties": {
                "tokens_removed": {
                    "type": "integer"
                }
            },
            "required": [
                "tokens_removed"
            ]
        }
    }
}