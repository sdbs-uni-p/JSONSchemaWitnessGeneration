{
  "allOf": [
    {
      "type": "object",
      "maxProperties": 2,
      "required": [
        "A",
        "Z"
      ],
      "patternProperties": {
        "^(A|Z)$": {
          "pattern": "c|a|b",
          "_patternStrong": "^a|b"
        }
      }
    },
    {
      "not": {
        "type": "object",
        "minProperties": 2,
        "additionalProperties": {
          "pattern": "a|b"
        }
      }
    }
  ]
}
