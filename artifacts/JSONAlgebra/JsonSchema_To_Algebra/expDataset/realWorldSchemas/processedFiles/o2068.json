{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "metadata_flattened.json",
    "description": "generate_metadata.py uses this schema to pull out data from users' metadata tsv's or xls's",
    "type": "object",
    "properties": {
        "program": {
            "type": "string"
        },
        "project": {
            "type": "string"
        },
        "center_name": {
            "type": "string"
        },
        "submitter_donor_id": {
            "type": "string"
        },
        "donor_uuid": {
            "type": "string"
        },
        "submitter_specimen_id": {
            "type": "string"
        },
        "specimen_uuid": {
            "type": "string"
        },
        "submitter_specimen_type": {
            "type": "string"
        },
        "submitter_sample_id": {
            "type": "string"
        },
        "sample_uuid": {
            "type": "string"
        },
        "analysis_type": {
            "type": "string"
        },
        "workflow_name": {
            "type": "string"
        },
        "workflow_version": {
            "type": "string"
        },
        "file_type": {
            "type": "string"
        },
        "file_path": {
            "type": "string"
        }
    }
}