{
    "name": "hosts",
    "connectorRef": {
        "bundleName": "org.forgerock.openicf.connectors.groovy-connector",
        "bundleVersion": "[1.4.0.0,2.0.0.0)",
        "connectorName": "org.forgerock.openicf.connectors.scriptedsql.ScriptedSQLConnector"
    },
    "producerBufferSize": 100,
    "connectorPoolingSupported": true,
    "poolConfigOption": {
        "maxObjects": 100,
        "maxIdle": 50,
        "maxWait": 150000,
        "minEvictableIdleTimeMillis": 120000,
        "minIdle": 10
    },
    "operationTimeout": {
        "CREATE": -1,
        "TEST": -1,
        "AUTHENTICATE": -1,
        "SEARCH": -1,
        "VALIDATE": -1,
        "GET": -1,
        "UPDATE": -1,
        "DELETE": -1,
        "SCRIPT_ON_CONNECTOR": -1,
        "SCRIPT_ON_RESOURCE": -1,
        "SYNC": -1,
        "SCHEMA": -1
    },
    "configurationProperties": {
        "username": "postgres",
        "password": "password",
        "driverClassName": "org.postgresql.Driver",
        "url": "jdbc:postgresql://SQLFIDDLE_HOST:5432/sqlfiddle",
        "autoCommit": true,
        "reloadScriptOnExecution": true,
        "createScriptFileName": "CreateScript.groovy",
        "testScriptFileName": "TestScript.groovy",
        "searchScriptFileName": "SearchScript.groovy",
        "updateScriptFileName": "UpdateScript.groovy",
        "deleteScriptFileName": "DeleteScript.groovy",
        "classpath": [
            "&{launcher.project.location}/tools/hosts"
        ]
    },
    "syncFailureHandler": {
        "maxRetries": 5,
        "postRetryAction": "logged-ignore"
    },
    "objectTypes": {
        "databases": {
            "$schema": "http://json-schema.org/draft-03/schema",
            "id": "schema_defs",
            "type": "object",
            "nativeType": "databases",
            "properties": {
                "db_type_id": {
                    "type": "integer",
                    "required": true,
                    "nativeName": "db_type_id",
                    "nativeType": "integer",
                    "flags": [
                        "NOT_UPDATEABLE"
                    ]
                },
                "jdbc_url": {
                    "type": "string",
                    "required": true,
                    "nativeName": "jdbc_url",
                    "nativeType": "string",
                    "flags": [
                        "NOT_CREATABLE",
                        "NOT_UPDATEABLE"
                    ]
                },
                "jdbc_class_name": {
                    "type": "string",
                    "required": true,
                    "nativeName": "jdbc_class_name",
                    "nativeType": "string",
                    "flags": [
                        "NOT_CREATABLE",
                        "NOT_UPDATEABLE"
                    ]
                },
                "schema_name": {
                    "type": "string",
                    "required": true,
                    "nativeName": "__NAME__",
                    "nativeType": "string",
                    "flags": [
                        "NOT_UPDATEABLE"
                    ]
                },
                "ddl": {
                    "type": "string",
                    "required": true,
                    "nativeName": "ddl",
                    "nativeType": "string",
                    "flags": [
                        "NOT_UPDATEABLE",
                        "NOT_READABLE",
                        "NOT_RETURNED_BY_DEFAULT"
                    ]
                },
                "statement_separator": {
                    "type": "string",
                    "required": true,
                    "nativeName": "statement_separator",
                    "nativeType": "string",
                    "flags": [
                        "NOT_UPDATEABLE",
                        "NOT_READABLE",
                        "NOT_RETURNED_BY_DEFAULT"
                    ]
                },
                "username": {
                    "type": "string",
                    "required": true,
                    "nativeName": "username",
                    "nativeType": "string",
                    "flags": [
                        "NOT_UPDATEABLE"
                    ]
                },
                "pw": {
                    "type": "string",
                    "required": true,
                    "nativeName": "pw",
                    "nativeType": "string",
                    "flags": [
                        "NOT_UPDATEABLE"
                    ]
                },
                "simple_name": {
                    "type": "string",
                    "required": false,
                    "nativeName": "simple_name",
                    "nativeType": "string",
                    "flags": [
                        "NOT_CREATABLE",
                        "NOT_UPDATEABLE"
                    ]
                },
                "full_name": {
                    "type": "string",
                    "required": false,
                    "nativeName": "full_name",
                    "nativeType": "string",
                    "flags": [
                        "NOT_CREATABLE",
                        "NOT_UPDATEABLE"
                    ]
                }
            }
        },
        "admin_databases": {
            "$schema": "http://json-schema.org/draft-03/schema",
            "id": "admin_databases",
            "type": "object",
            "nativeType": "admin_databases",
            "properties": {
                "db_type_id": {
                    "type": "integer",
                    "required": true,
                    "nativeName": "db_type_id",
                    "nativeType": "integer",
                    "flags": [
                        "NOT_UPDATEABLE"
                    ]
                },
                "jdbc_url": {
                    "type": "string",
                    "required": true,
                    "nativeName": "jdbc_url",
                    "nativeType": "string",
                    "flags": [
                        "NOT_CREATABLE",
                        "NOT_UPDATEABLE"
                    ]
                },
                "jdbc_class_name": {
                    "type": "string",
                    "required": true,
                    "nativeName": "jdbc_class_name",
                    "nativeType": "string",
                    "flags": [
                        "NOT_CREATABLE",
                        "NOT_UPDATEABLE"
                    ]
                },
                "schema_name": {
                    "type": "string",
                    "required": true,
                    "nativeName": "__NAME__",
                    "nativeType": "string",
                    "flags": [
                        "NOT_UPDATEABLE"
                    ]
                },
                "default_database": {
                    "type": "string",
                    "required": true,
                    "nativeName": "default_database",
                    "nativeType": "string",
                    "flags": [
                        "NOT_CREATABLE",
                        "NOT_UPDATEABLE"
                    ]
                },
                "admin_username": {
                    "type": "string",
                    "required": true,
                    "nativeName": "admin_username",
                    "nativeType": "string",
                    "flags": [
                        "UPDATEABLE"
                    ]
                },
                "admin_password": {
                    "type": "string",
                    "required": true,
                    "nativeName": "admin_password",
                    "nativeType": "string",
                    "flags": [
                        "UPDATEABLE"
                    ]
                },
                "simple_name": {
                    "type": "string",
                    "required": false,
                    "nativeName": "simple_name",
                    "nativeType": "string",
                    "flags": [
                        "NOT_CREATABLE",
                        "NOT_UPDATEABLE"
                    ]
                },
                "full_name": {
                    "type": "string",
                    "required": false,
                    "nativeName": "full_name",
                    "nativeType": "string",
                    "flags": [
                        "NOT_CREATABLE",
                        "NOT_UPDATEABLE"
                    ]
                },
                "jdbc_url_template": {
                    "type": "string",
                    "required": true,
                    "nativeName": "jdbc_url_template",
                    "nativeType": "string",
                    "flags": [
                        "NOT_CREATABLE",
                        "NOT_UPDATEABLE"
                    ]
                }
            }
        }
    },
    "operationOptions": {}
}