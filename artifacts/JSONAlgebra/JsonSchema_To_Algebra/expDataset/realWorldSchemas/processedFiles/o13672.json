{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "course": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "integer"
                },
                "number": {
                    "type": "string"
                },
                "title": {
                    "type": "string"
                },
                "department_id": {
                    "type": "integer"
                }
            }
        },
        "quarter": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "integer"
                },
                "year": {
                    "type": "integer"
                },
                "name": {
                    "type": "string"
                },
                "current": {
                    "type": "boolean"
                }
            }
        },
        "professor": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "integer"
                },
                "first_name": {
                    "type": "string"
                },
                "last_name": {
                    "type": "string"
                }
            }
        },
        "user_posted": {
            "type": "boolean"
        }
    }
}