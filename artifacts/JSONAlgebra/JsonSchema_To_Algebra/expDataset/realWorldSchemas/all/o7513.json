{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "description": "Schema for simple date response at http://date.jsontest.com",
    "title": "Date Response",
    "type": "object",
    "properties": {
        "time": {
            "type": "string"
        },
        "milliseconds_since_epoch": {
            "type": "integer"
        },
        "date": {
            "type": "string"
        }
    },
    "required": [
        "time",
        "milliseconds_since_epoch",
        "date"
    ]
}