{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "city": {
            "type": "string"
        },
        "state": {
            "type": "string"
        },
        "zip": {
            "type": "string"
        },
        "country": {
            "type": "string"
        }
    },
    "patternProperties": {
        "^line[1-3]$": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "city",
        "state",
        "zip",
        "country",
        "line1"
    ]
}