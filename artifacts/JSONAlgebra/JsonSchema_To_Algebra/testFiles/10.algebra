def "metapackage" = {
	props[
		"^type$": {
			type[
				str
			],
			enum[
				"metapackage"
			]
		};
		
	],
	req[
		"name",
		"version",
		"type"
	]
},
def "package" = {
	props[
		"^dist$": type[
			obj
		],
		"^source$": type[
			obj
		];
		
	],
	oneOf[
		req[
			"name",
			"version",
			"source"
		],
		req[
			"name",
			"version",
			"dist"
		]
	]
},
def "versions" = {
	type[
		obj
	]
},
def "provider" = {
	type[
		obj
	],
	props[
		"^sha256$": type[
			str
		];
		
	]
},
rootdef "root" = {
	oneOf[
		req[
			"packages"
		],
		req[
			"providers"
		],
		req[
			"provider-includes",
			"providers-url"
		]
	],
	type[
		obj
	],
	props[
		"^providers-url$": type[
			str
		],
		"^packages$": {
			type[
				obj,
				arr
			]
		},
		"^search$": type[
			str
		],
		"^warning$": type[
			str
		],
		"^notify-batch$": type[
			str
		],
		"^providers$": {
			type[
				obj
			]
		},
		"^provider-includes$": {
			type[
				obj
			]
		};
		
	]
},
def "version" = {
	type[
		obj
	]
},
def "package-base" = props[
	"^type$": type[
		str
	],
	"^version$": type[
		str
	],
	"^version_normalized$": type[
		str
	],
	"^autoload$": type[
		obj
	],
	"^replace$": type[
		obj
	],
	"^time$": type[
		str
	],
	"^conflict$": type[
		obj
	],
	"^require$": type[
		obj
	],
	"^provide$": type[
		obj
	],
	"^name$": type[
		str
	];
	true
]