            "jet_type": {
                "enum": [
                    "AK4PF",
                    "AK5PF",
                    "AK6PF",
                    "AK7PF",
                    "AK8PF",
                    "AK4Calo",
                    "AK5Calo",
                    "AK6Calo",
                    "AK7Calo",
                    "AK8Calo",
                    "KT4PF",
                    "KT5PF",
                    "KT6PF",
                    "KT7PF",
                    "KT8PF",
                    "KT4Calo",
                    "KT5Calo",
                    "KT6Calo",
                    "KT7Calo",
                    "KT8Calo"
                ],
                "type": "string",
                "title": "Jet type"
            },
            "muon_type": {
                "enum": [
                    "PFMuon",
                    "GlobalMuon",
                    "TrackerMuon"
                ],
                "type": "string",
                "title": "Muon type"
            },
            "photon_type": {
                "enum": [
                    "Photon",
                    "Conversion"
                ],
