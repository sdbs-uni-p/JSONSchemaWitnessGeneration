{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "heading",
    "description": "A heading atom",
    "type": "object",
    "properties": {
        "titleTag": {
            "description": "Heading level",
            "enum": [
                "h1",
                "h2",
                "h3"
            ]
        },
        "title": {
            "type": "string"
        },
        "subTitle": {
            "type": "string"
        }
    },
    "required": [
        "title"
    ]
}