{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Select2 Auto-complete Data Schema",
    "description": "",
    "type": "array",
    "items": {
        "oneOf": [
            {
                "type": "string"
            },
            {
                "type": "object",
                "properties": {
                    "label": {
                        "type": "string"
                    },
                    "restype": {
                        "type": "string"
                    },
                    "router": {
                        "type": "string"
                    },
                    "category": {
                        "type": "string",
                        "enum": [
                            "File",
                            "Notes",
                            "Development",
                            "Personal"
                        ]
                    }
                },
                "required": [
                    "label"
                ],
                "additionalProperties": false
            }
        ]
    }
}