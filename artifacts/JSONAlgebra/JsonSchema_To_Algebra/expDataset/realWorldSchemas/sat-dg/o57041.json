{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "blinkleds",
    "description": "led on or off",
    "type": "object",
    "properties": {
        "leds": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "id": {
                        "description": "the identification of the led",
                        "type": "string"
                    },
                    "status": {
                        "description": "the status of the led",
                        "enum": [
                            "on",
                            "off"
                        ]
                    }
                },
                "required": [
                    "id",
                    "status"
                ]
            },
            "minItems": 1
        }
    },
    "required": [
        "leds"
    ]
}