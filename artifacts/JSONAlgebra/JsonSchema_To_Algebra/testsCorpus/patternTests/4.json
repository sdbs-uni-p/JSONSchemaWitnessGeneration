{
  "type": "object",
  "properties": {
    "domain": {
      "type": "string",
      "pattern": "^([a-z0-9]+(-[a-z0-9]+)*\\.)+[a-z]{2,}$"
    },
    "url": {
      "type": "string",
      "format": "url"
    },
    "selector": {
      "type": "string"
    }
  },
  "required": [
    "domain",
    "url",
    "selector"
  ],
  "additionalProperties": false
}
