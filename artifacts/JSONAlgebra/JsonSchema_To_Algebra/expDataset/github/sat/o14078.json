                        "type": "object",
                        "properties": {
                          "registration": {
                            "type": "string",
                            "name": "registration",
                            "title": "Registration",
                            "description": "The registration code",
                            "example": "NZCG-2345"
                          },
                          "description": {
                            "type": "string",
                            "name": "description",
                            "title": "Description",
                            "description": "The registration description",
                            "example": "Nelson Coast Guard Membership"
                          }
                        }
                      }
                    },
                    "other": {
                      "type": "array",
                      "minItems": 0,
                      "_uniqueItems": true,
                      "additionalItems": true,
                      "format": "table",
                      "name": "other",
                      "title": "Other",
                      "description": "Other registration or permits for the vessel.",
                      "items": {
                        "type": "object",
                        "properties": {
                          "registration": {
                            "type": "string",
                            "name": "registration",
                            "title": "Registration",
                            "description": "The registration code",
                            "example": "DOC-2345"
                          },
                          "description": {
                            "type": "string",
                            "name": "description",
                            "title": "Description",
                            "description": "The registration description",
                            "example": "Tasmanian National Parks Access Permit 2015"
                          }
                        }
                      }
                    }
                  },
                  "required": [
                    "imo",
                    "national",
                    "local",
                    "other"
                  ]
