{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "name": {
            "type": "string"
        },
        "description": {
            "type": "string"
        },
        "icon": {
            "type": "string"
        },
        "periodic_booking": {
            "type": "boolean"
        },
        "is_available": {
            "type": "boolean"
        },
        "was_available": {
            "description": "Previous value of is_available",
            "type": "boolean"
        },
        "min_delay": {
            "type": "integer",
            "minimum": -1
        },
        "max_delay": {
            "type": "integer",
            "minimum": -1
        },
        "type_id": {
            "type": "integer"
        },
        "color": {
            "type": "string"
        },
        "validation": {
            "type": "boolean"
        }
    },
    "required": [
        "type_id",
        "is_available",
        "was_available"
    ],
    "additionalProperties": false
}