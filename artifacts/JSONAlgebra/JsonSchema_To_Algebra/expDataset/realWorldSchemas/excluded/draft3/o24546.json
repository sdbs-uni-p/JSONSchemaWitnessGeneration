{
    "$linkedData": {
        "books": {
            "path": [
                "/bibles",
                "/{bibleId}",
                "/books"
            ],
            "schema": "book",
            "type": "collection"
        }
    },
    "description": "Holy bible.",
    "properties": {
        "bibleId": {
            "required": true,
            "type": "integer"
        },
        "copyright": {
            "required": false,
            "type": "string"
        },
        "edition": {
            "required": false,
            "type": "string"
        },
        "eletronicTranscriptionSource": {
            "required": false,
            "type": "string"
        },
        "eletronicTranscriptionSourceUrl": {
            "required": false,
            "type": "string"
        },
        "language": {
            "required": true,
            "type": "string"
        },
        "name": {
            "required": true,
            "type": "string"
        },
        "printedSource": {
            "required": true,
            "type": "string"
        },
        "url": {
            "required": false,
            "type": "string"
        }
    },
    "type": "object"
}