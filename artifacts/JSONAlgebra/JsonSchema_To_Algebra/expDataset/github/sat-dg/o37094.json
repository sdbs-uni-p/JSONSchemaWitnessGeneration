{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://mcda.drugis.org/relativeEntry.json#",
    "title": "MCDA performance table entry for relative data",
    "type": "object",
    "required": [
        "criterion",
        "dataSource",
        "performance"
    ],
    "properties": {
        "criterion": {
            "type": "string"
        },
        "dataSource": {
            "type": "string"
        },
        "performance": {
            "type": "object",
            "required": [
                "distribution"
            ],
            "properties": {
                "distribution": {
                    "type": "object",
                    "required": [
                        "type",
                        "parameters"
                    ],
                    "properties": {
                        "type": {
                            "type": "string",
                            "enum": [
                                "relative-identity-normal",
                                "relative-logit-normal",
                                "relative-log-normal",
                                "relative-cloglog-normal",
                                "relative-smd-normal",
                                "relative-normal",
                                "relative-survival"
                            ]
                        },
                        "parameters": {
                            "type": "object",
                            "required": [
                                "baseline",
                                "relative"
                            ],
                            "properties": {
                                "baseline": {
                                    "type": "object",
                                    "required": [
                                        "type",
                                        "name"
                                    ],
                                    "additionalProperties": false,
                                    "properties": {
                                        "type": {
                                            "type": "string",
                                            "enum": [
                                                "dnorm",
                                                "dsurv",
                                                "dbeta-logit",
                                                "dbeta-cloglog",
                                                "dt",
                                                "identity",
                                                "logit",
                                                "log",
                                                "cloglog",
                                                "smd"
                                            ]
                                        },
                                        "name": {
                                            "type": "string"
                                        },
                                        "mu": {
                                            "type": "number"
                                        },
                                        "sigma": {
                                            "type": "number"
                                        },
                                        "distribution": {
                                            "type": "string",
                                            "enum": [
                                                "Normal",
                                                "Beta",
                                                "Gamma"
                                            ]
                                        },
                                        "title": {
                                            "type": "string"
                                        },
                                        "property": {
                                            "type": "string",
                                            "enum": [
                                                "log odds ratio",
                                                "mean difference",
                                                "log hazard ratio"
                                            ]
                                        },
                                        "alpha": {
                                            "type": "number"
                                        },
                                        "beta": {
                                            "type": "number"
                                        },
                                        "summaryMeasure": {
                                            "type": "string",
                                            "enum": [
                                                "none",
                                                "median",
                                                "mean",
                                                "survivalAtTime"
                                            ]
                                        },
                                        "survivalAtTime": {
                                            "type": "number"
                                        },
                                        "scale": {
                                            "type": "string",
                                            "enum": [
                                                "mean difference",
                                                "hazard ratio",
                                                "hazard",
                                                "log odds",
                                                "mean"
                                            ]
                                        },
                                        "std.err": {
                                            "type": "number"
                                        },
                                        "stdErr": {
                                            "type": "number"
                                        },
                                        "mean": {
                                            "type": "number"
                                        },
                                        "dof": {
                                            "type": "number"
                                        }
                                    }
                                },
                                "relative": {
                                    "type": "object",
                                    "required": [
                                        "mu",
                                        "cov",
                                        "type"
                                    ],
                                    "properties": {
                                        "type": {
                                            "type": "string",
                                            "enum": [
                                                "dmnorm"
                                            ]
                                        },
                                        "mu": {
                                            "type": "object",
                                            "additionalProperties": {
                                                "type": "number"
                                            }
                                        },
                                        "cov": {
                                            "type": "object",
                                            "required": [
                                                "colnames",
                                                "rownames",
                                                "data"
                                            ],
                                            "properties": {
                                                "colnames": {
                                                    "type": "array",
                                                    "items": {
                                                        "type": "string"
                                                    }
                                                },
                                                "rownames": {
                                                    "type": "array",
                                                    "items": {
                                                        "type": "string"
                                                    }
                                                },
                                                "data": {
                                                    "type": "array",
                                                    "items": {
                                                        "type": "array",
                                                        "items": {
                                                            "type": "number"
                                                        }
                                                    }
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}