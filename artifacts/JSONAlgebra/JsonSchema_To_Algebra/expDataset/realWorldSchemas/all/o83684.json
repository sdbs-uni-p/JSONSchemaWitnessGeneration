{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Chargeback finance",
    "description": "Chargeback messages for Finance division",
    "type": "object",
    "properties": {
        "functionCode": {
            "type": "string"
        },
        "usageUnits": {
            "type": "integer"
        },
        "division": {
            "type": "integer",
            "minimum": 15,
            "maximum": 30,
            "default": 15
        }
    },
    "required": [
        "functionCode",
        "usageUnits",
        "division"
    ]
}