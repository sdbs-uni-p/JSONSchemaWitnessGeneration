{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "array",
    "items": {
        "type": "object",
        "properties": {
            "id": {
                "type": "integer",
                "minimum": 1
            },
            "categoryId": {
                "type": "integer",
                "minimum": 1
            },
            "amount": {
                "type": "number",
                "faker": "finance.amount"
            },
            "description": {
                "type": "string"
            },
            "createdAt": {
                "type": "string",
                "faker": "date.past"
            },
            "createdBy": {
                "type": "integer",
                "minimum": 1
            }
        },
        "required": [
            "id",
            "categoryId",
            "amount",
            "description",
            "createdAt",
            "createdBy"
        ]
    }
}