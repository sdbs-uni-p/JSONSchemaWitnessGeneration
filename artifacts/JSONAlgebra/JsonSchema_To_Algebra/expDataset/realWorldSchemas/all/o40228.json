{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "registrationID": {
            "type": "string",
            "pattern": "[a-zA-Z0-9_-]{162}"
        },
        "macAddress": {
            "type": "string",
            "pattern": "([0-9a-fA-F][0-9a-fA-F](:|-)){4}([0-9a-fA-F][0-9a-fA-F])"
        }
    },
    "required": [
        "registrationID",
        "macAddress"
    ]
}