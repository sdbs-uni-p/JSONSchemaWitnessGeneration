{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "identifier": {
            "alias": "array",
            "items": {
                "alias-name": {
                    "type": "string"
                },
                "ip": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "prefix": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "port-range": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "lower-port": "integer",
                            "upper-port": "integer"
                        }
                    }
                },
                "traffic-protocol": {
                    "type": "array",
                    "items": {
                        "type": "integer"
                    }
                },
                "FQDN": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "URI": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                }
            }
        }
    }
}