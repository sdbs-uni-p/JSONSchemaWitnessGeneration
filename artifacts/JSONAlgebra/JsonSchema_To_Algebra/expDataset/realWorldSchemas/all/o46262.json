{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Security Holding",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "marketValue": {
            "type": "number"
        },
        "price": {
            "type": "number"
        },
        "quantity": {
            "type": "number"
        },
        "security": {
            "$ref": "#/definitions/Security"
        }
    },
    "definitions": {
        "Security": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "_id": {
                    "type": "integer",
                    "description": "A unique ID for Security"
                },
                "name": {
                    "type": "string"
                },
                "symbol": {
                    "type": "string"
                }
            }
        }
    }
}