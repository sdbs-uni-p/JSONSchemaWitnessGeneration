{
    "additionalProperties": false,
    "description": "Schema for a Google Analytics Ecommerce transaction item",
    "properties": {
        "category": {
            "maxLength": 500,
            "type": "string"
        },
        "code": {
            "maxLength": 500,
            "type": "string"
        },
        "currency": {
            "maxLength": 3,
            "minLength": 3,
            "type": "string"
        },
        "name": {
            "maxLength": 500,
            "type": "string"
        },
        "price": {
            "multipleOf": 0.01,
            "type": "number"
        },
        "quantity": {
            "type": "integer"
        }
    },
    "self": {
        "format": "jsonschema",
        "name": "item",
        "vendor": "com.google.analytics.ecommerce",
        "version": "1-0-0"
    },
    "type": "object"
}