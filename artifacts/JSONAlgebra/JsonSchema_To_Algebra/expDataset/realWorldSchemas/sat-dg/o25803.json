        "description": {
            "_$id": "/properties/description",
            "description": "",
            "oneOf": [
                {
                    "type": "null"
                },
                {
                    "contentMediaType": "text/html",
                    "type": "string"
                }
            ],
            "title": "Description"
        },
        "extra": {
            "_$id": "/properties/extra",
            "description": "object data extensions (JSON format)",
            "oneOf": [
                {
                    "type": "null"
                },
                {
                    "type": "object"
                }
            ],
            "title": "Extra"
        },
        "lang": {
            "_$id": "/properties/lang",
            "description": "language used, ISO 639-3 code",
            "oneOf": [
                {
                    "type": "null"
                },
                {
                    "type": "string"
                }
            ],
            "title": "Lang"
