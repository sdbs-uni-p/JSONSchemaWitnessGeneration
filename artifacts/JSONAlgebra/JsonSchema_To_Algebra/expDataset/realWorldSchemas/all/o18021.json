{
    "description": "A single entry in my CV. Features place, time and identifiers for the events.",
    "properties": {
        "@context": {
            "const": "http://schema.org"
        },
        "@type": {
            "const": "Event"
        },
        "cat": {
            "type": "string"
        },
        "description": {
            "type": "string"
        },
        "endDate": {
            "format": "ISO8601",
            "type": "string"
        },
        "endLabel": {
            "type": "string"
        },
        "location": {
            "properties": {
                "@type": {
                    "const": "Place"
                },
                "coord": {
                    "format": "ISO6709",
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "url": {
                    "format": "uri",
                    "type": "string"
                }
            },
            "type": "string"
        },
        "name": {
            "type": "string"
        },
        "startDate": {
            "format": "ISO8601",
            "type": "string"
        },
        "startLabel": {
            "type": "string"
        },
        "uri": {
            "format": "uri",
            "type": "string"
        }
    },
    "title": "cv-item",
    "type": "object"
}