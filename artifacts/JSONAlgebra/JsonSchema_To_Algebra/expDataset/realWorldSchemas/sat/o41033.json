{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "properties": {
        "DestinationCidrBlock": {
            "pattern": "^(([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])\\.){3}([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])(/(\\d|[1-2]\\d|3[0-2]))|0\\.0\\.0\\.0\\/0$",
            "type": "string"
        },
        "NatGatewayId": {
            "pattern": "^[a-z][a-z0-9\\-]+$",
            "type": "string"
        },
        "RouteTableId": {
            "pattern": "^[a-z][a-z0-9\\-]+$",
            "type": "string"
        }
    },
    "required": [
        "DestinationCidrBlock",
        "RouteTableId",
        "NatGatewayId"
    ],
    "type": "object"
}