{
    "title": "Bureau",
    "$schema": "http://json-schema.org/draft-04/schema#",
    "description": "Bureau represents a FCC Bureau or Office",
    "required": [
        "_metadata",
        "bureau"
    ],
    "type": "object",
    "properties": {
        "_metadata": {
            "required": [
                "request",
                "response"
            ],
            "type": "object",
            "properties": {
                "request": {
                    "required": [
                        "ip",
                        "params",
                        "time",
                        "user"
                    ],
                    "type": "object",
                    "properties": {
                        "ip": {
                            "type": "string"
                        },
                        "params": {
                            "type": "null"
                        },
                        "user": {
                            "type": "null"
                        },
                        "time": {
                            "type": "string"
                        }
                    }
                },
                "response": {
                    "required": [
                        "time"
                    ],
                    "type": "object",
                    "properties": {
                        "time": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "bureau": {
            "required": [
                "bureau_code",
                "bureau_name",
                "edocs_bureau_code",
                "id"
            ],
            "type": "object",
            "properties": {
                "bureau_name": {
                    "type": "string"
                },
                "id": {
                    "type": "integer"
                },
                "edocs_bureau_code": {
                    "type": "null"
                },
                "bureau_code": {
                    "type": "string"
                }
            }
        }
    }
}