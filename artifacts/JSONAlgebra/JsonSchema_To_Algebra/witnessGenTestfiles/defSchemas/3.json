{
  "$schema": "http://json-schema.org/draft-06/schema",
  "definitions": {

    "x": {
      "type": "object",
      "anyOf": [
        {"patternProperties": { "^a":  { "$ref": "#/definitions/l" }}},
        {"patternProperties": { "^b":  { "$ref": "#/definitions/y" }}}
      ]
    },


    "y": {
      "type": "object",
      "patternProperties": {"^c": { "$ref": "#/definitions/m" }},
      "anyOf": [
        {"patternProperties": { "^a":  { "$ref": "#/definitions/z" }}} ,
        {"patternProperties": { "^b":  { "$ref": "#/definitions/k" }}}
      ]
    },


    "k": {
      "type": "object",
      "patternProperties": {"^a": { "$ref": "#/definitions/l" }}
    },


    "l": {
      "type": "object",
      "minProperties": 0,
      "maxProperties": 0,
      "patternProperties": {"^a": { "$ref": "#/definitions/x" }}
    },


    "z": {
      "anyOf": [
        {
          "type": "object",
          "patternProperties": {"^a": { "$ref": "#/definitions/x" }}
        },
        {"type": "null"}
      ]
    },


    "m": {"type": "number"}

  },


  "type": "object",

  "properties": {
    "x": { "$ref": "#/definitions/x" },
    "y": { "$ref": "#/definitions/y" },
    "k": { "$ref": "#/definitions/k" },
    "l": { "$ref": "#/definitions/l" },
    "z": { "$ref": "#/definitions/z" },
    "m": { "$ref": "#/definitions/m" }
  },
  "required": ["x", "y","k","l","z","m"]
}
