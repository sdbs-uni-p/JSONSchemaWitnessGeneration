{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "additionalProperties": false,
    "title": "notification content",
    "description": "notification content",
    "type": "object",
    "properties": {
        "from": {
            "type": "string"
        },
        "to": {
            "type": "string"
        },
        "message": {
            "type": "string"
        },
        "priority": {
            "type": "integer",
            "minimum": 0,
            "maximum": 10
        }
    },
    "required": [
        "to",
        "message",
        "priority"
    ]
}