    "compositionWithFeaturesDefined": {
        "name": "compositionWithFeaturesDefined",
        "definition": {
            "features": {
                "$schema": "http://json-schema.org/draft-04/schema#",
                "type": "object",
                "properties": {
                    "close": {
                        "type": "object",
                        "properties": {
                            "onActions": {
                                "type": "array",
                                "items": {
                                    "type": "string"
                                }
                            }
                        }
                    },
                    "something": {
                        "type": "object",
                        "properties": {
                            "resource": {
                                "type": "string",
                                "default": "${topic:myResource}"
                            }
                        },
                        "default": {
                            "resource": "${topic:myResource}"
                        }
                    }
                }
            },
            "areas": {
                ".": [
                    {
                        "widget": "laxarjs/test_widget1",
                        "id": "idx1",
                        "features": {
                            "open": {
                                "onActions": [
                                    "openAction"
                                ]
                            },
                            "close": {
                                "onActions": "${features.close.onActions}"
                            }
                        }
                    }
                ],
                "areaX": [
                    {
                        "widget": "laxarjs/test_widget2",
                        "features": {
                            "importantFeature": {
                                "resource": "${features.something.resource}",
                                "attribute": "entries"
                            }
                        }
                    }
                ]
            }
        }
    },
