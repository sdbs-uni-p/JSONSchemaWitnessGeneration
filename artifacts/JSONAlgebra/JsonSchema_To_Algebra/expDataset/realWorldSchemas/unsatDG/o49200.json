{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://jerzual.github.io/azimut/config.json",
    "type": "object",
    "title": "ConfigurationSchema",
    "required": [
        "backendurl"
    ],
    "description": "Environment configuration",
    "additionalProperties": false,
    "properties": {
        "backendUrl": {
            "$id": "#/properties/backendUrl",
            "type": "string",
            "default": "",
            "examples": [
                "http://azimut_server"
            ],
            "description": "Base url",
            "pattern": "^https?://(.*)$"
        },
        "databaseHost": {
            "$id": "#/properties/databaseHost",
            "type": "string",
            "description": "host of couchdb",
            "default": ""
        }
    }
}