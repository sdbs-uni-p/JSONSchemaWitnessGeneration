{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "https://mcda.drugis.org/betaDistribution.json#",
    "title": "MCDA beta distribution for the performance table entry of absolute data",
    "type": "object",
    "required": [
        "type",
        "parameters"
    ],
    "additionalProperties": false,
    "properties": {
        "type": {
            "type": "string",
            "enum": [
                "dbeta"
            ]
        },
        "parameters": {
            "type": "object",
            "additionalProperties": false,
            "required": [
                "alpha",
                "beta"
            ],
            "properties": {
                "alpha": {
                    "type": "number"
                },
                "beta": {
                    "type": "number"
                }
            }
        },
        "input": {
            "required": [
                "events",
                "sampleSize"
            ],
            "additionalProperties": false,
            "properties": {
                "events": {
                    "type": "number"
                },
                "sampleSize": {
                    "type": "number"
                }
            }
        }
    }
}