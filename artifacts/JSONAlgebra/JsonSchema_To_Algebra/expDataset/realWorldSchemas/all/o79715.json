{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "status": {
            "type": "string",
            "enum": [
                "initialized",
                "running",
                "running_canceled",
                "stopping",
                "stopping_canceled",
                "stopped",
                "shutdown",
                "configerror"
            ],
            "default": "initialized",
            "description": "der Status der StartStoppApplikation"
        }
    },
    "required": [
        "status"
    ]
}