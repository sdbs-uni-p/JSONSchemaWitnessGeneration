{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "minItems": 1,
    "_uniqueItems": true,
    "items": {
        "type": "object",
        "properties": {
            "headers": {
                "type": "object",
                "properties": {
                    "user-agent": {
                        "type": "string"
                    }
                }
            },
            "device": {
                "type": "object",
                "properties": {
                    "deviceName": {
                        "anyOf": [
                            {
                                "type": "string"
                            },
                            {
                                "type": "null"
                            }
                        ]
                    },
                    "marketingName": {
                        "anyOf": [
                            {
                                "type": "string"
                            },
                            {
                                "type": "null"
                            }
                        ]
                    },
                    "manufacturer": {
                        "type": "string"
                    },
                    "brand": {
                        "type": "string"
                    },
                    "display": {
                        "type": "object",
                        "properties": {
                            "width": {
                                "anyOf": [
                                    {
                                        "type": "integer",
                                        "minimum": 0
                                    },
                                    {
                                        "type": "null"
                                    }
                                ]
                            },
                            "height": {
                                "anyOf": [
                                    {
                                        "type": "integer",
                                        "minimum": 0
                                    },
                                    {
                                        "type": "null"
                                    }
                                ]
                            },
                            "touch": {
                                "anyOf": [
                                    {
                                        "type": "boolean"
                                    },
                                    {
                                        "type": "null"
                                    }
                                ]
                            },
                            "type": {
                                "type": "string"
                            },
                            "size": {
                                "anyOf": [
                                    {
                                        "type": "number",
                                        "minimum": 0
                                    },
                                    {
                                        "type": "null"
                                    }
                                ]
                            }
                        },
                        "required": [
                            "width",
                            "height",
                            "touch",
                            "type",
                            "size"
                        ]
                    },
                    "dualOrientation": {
                        "type": "boolean"
                    },
                    "type": {
                        "type": "string"
                    },
                    "simCount": {
                        "type": "integer",
                        "minimum": 0,
                        "multipleOf": 1
                    },
                    "market": {
                        "type": "object",
                        "properties": {
                            "regions": {
                                "type": "array",
                                "items": {
                                    "type": "string"
                                }
                            },
                            "countries": {
                                "type": "array",
                                "items": {
                                    "type": "string"
                                }
                            },
                            "vendors": {
                                "type": "array",
                                "items": {
                                    "type": "string"
                                }
                            }
                        },
                        "required": [
                            "regions",
                            "countries",
                            "vendors"
                        ]
                    }
                },
                "required": [
                    "deviceName",
                    "marketingName",
                    "manufacturer",
                    "brand",
                    "display",
                    "dualOrientation",
                    "type",
                    "simCount",
                    "market"
                ]
            },
            "browser": {
                "type": "object",
                "properties": {
                    "name": {
                        "anyOf": [
                            {
                                "type": "string"
                            },
                            {
                                "type": "null"
                            }
                        ]
                    },
                    "modus": {
                        "anyOf": [
                            {
                                "type": "null"
                            },
                            {
                                "type": "integer"
                            }
                        ]
                    },
                    "version": {
                        "type": "string"
                    },
                    "manufacturer": {
                        "type": "string"
                    },
                    "bits": {
                        "type": "integer"
                    },
                    "type": {
                        "type": "string"
                    }
                },
                "required": [
                    "name",
                    "modus",
                    "version",
                    "manufacturer",
                    "bits",
                    "type"
                ]
            },
            "os": {
                "type": "object",
                "properties": {
                    "name": {
                        "anyOf": [
                            {
                                "type": "string"
                            },
                            {
                                "type": "null"
                            }
                        ]
                    },
                    "marketingName": {
                        "anyOf": [
                            {
                                "type": "string"
                            },
                            {
                                "type": "null"
                            }
                        ]
                    },
                    "version": {
                        "type": "string"
                    },
                    "manufacturer": {
                        "type": "string"
                    },
                    "bits": {
                        "type": "integer"
                    }
                },
                "required": [
                    "name",
                    "marketingName",
                    "version",
                    "manufacturer",
                    "bits"
                ]
            },
            "engine": {
                "type": "object",
                "properties": {
                    "name": {
                        "anyOf": [
                            {
                                "type": "string"
                            },
                            {
                                "type": "null"
                            }
                        ]
                    },
                    "version": {
                        "type": "string"
                    },
                    "manufacturer": {
                        "type": "string"
                    }
                },
                "required": [
                    "name",
                    "version",
                    "manufacturer"
                ]
            }
        },
        "required": [
            "headers",
            "device",
            "browser",
            "os",
            "engine"
        ]
    }
}