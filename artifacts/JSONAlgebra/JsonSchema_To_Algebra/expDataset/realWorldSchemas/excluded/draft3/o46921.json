{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "File schema",
    "type": "array",
    "items": {
        "type": "object",
        "patternProperties": {
            "^\\w{3,3}$": {
                "required": true,
                "type": "number",
                "minimum": 1
            }
        },
        "additionalProperties": false
    },
    "uniqueItems": true
}