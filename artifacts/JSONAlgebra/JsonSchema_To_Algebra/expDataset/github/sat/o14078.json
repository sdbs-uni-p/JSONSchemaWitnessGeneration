          "name": "server",
          "format": "categories",
          "basicCategoryTitle": "Version",
          "properties": {
            "vessel": {
              "id": "http://jsonschema.net/signalk/config/server/vessel",
              "type": "object",
              "additionalProperties": false,
              "title": "Vessel",
              "description": "",
              "name": "vessel",
              "properties": {
                "uuid": {
                  "id": "http://jsonschema.net/signalk/config/server/vessel/uuid",
                  "type": "string",
                  "minLength": 1,
                  "title": "UUID (self)",
                  "description": "Enter uuid (unique vessel identifier)",
                  "name": "uuid",
                  "default": "self"
                },
                "name": {
                  "id": "http://jsonschema.net/signalk/config/server/vessel/name",
                  "type": "string",
                  "minLength": 1,
                  "title": "Name",
                  "description": "The common name of the vessel",
                  "name": "name",
                  "default": "No name"
                },
                "mmsi": {
                  "id": "http://jsonschema.net/signalk/config/server/vessel/mmsi",
                  "type": "string",
                  "minLength": 1,
                  "title": "MMSI",
                  "description": "Enter MMSI number",
                  "name": "mmsi"
                },
                "flag": {
                  "id": "http://jsonschema.net/signalk/config/server/vessel/flag",
                  "type": "string",
                  "name": "flag",
                  "title": "Flag",
                  "description": "The country of ship registration, or flag state of the vessel",
                  "example": "NZ"
                },
                "port": {
                  "id": "http://jsonschema.net/signalk/config/server/vessel/port",
                  "type": "string",
                  "name": "port",
                  "title": "Port",
                  "description": "The home port of the vessel",
                  "example": "Nelson"
                },
                "registrations": {
                  "type": "object",
                  "name": "registrations",
                  "title": "Registrations",
                  "format": "categories",
                  "basicCategoryTitle": "IMO",
                  "description": "The various registrations of the vessel.",
                  "example": "eg for a New Zealand Part B recreational vessel: 'NZ654'",
                  "properties": {
                    "imo": {
                      "type": "string",
                      "name": "imo",
                      "title": "IMO",
                      "description": "The IMO number of the vessel.",
                      "example": "IMO 9074729",
                      "maxLength": 11,
                      "minLength": 11
                    },
                    "national": {
                      "type": "array",
                      "minItems": 0,
                      "_uniqueItems": true,
                      "additionalItems": true,
                      "name": "national",
                      "title": "National",
                      "format": "table",
                      "description": "The national registration number of the vessel.",
                      "items": {
                        "type": "object",
                        "properties": {
                          "country": {
                            "type": "string",
                            "name": "country",
                            "title": "Country",
                            "description": "The ISO 3166-2 country code.",
                            "example": "NZ",
                            "maxLength": 2,
                            "minLength": 2
                          },
                          "registration": {
                            "type": "string",
                            "name": "registration",
                            "title": "Registration",
                            "description": "The registration code",
                            "example": "NZ654"
                          },
                          "description": {
                            "type": "string",
                            "title": "Description",
                            "name": "description",
                            "description": "The registration description",
                            "example": "New Zealand Part B recreational vessel"
                          }
                        }
                      }
                    },
                    "local": {
                      "type": "array",
                      "minItems": 0,
                      "_uniqueItems": true,
                      "additionalItems": true,
                      "format": "table",
                      "name": "local",
