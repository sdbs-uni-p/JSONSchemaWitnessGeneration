{
    "id": "http://wordnik.github.io/schemas/v1.2/parameterObject.json#",
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "paramType": {
            "enum": [
                "path",
                "query",
                "body",
                "header",
                "form"
            ]
        },
        "name": {
            "type": "string",
            "required": true
        },
        "type": {
            "type": "string"
        },
        "description": {
            "type": "string"
        },
        "required": {
            "type": "boolean"
        },
        "allowMultiple": {
            "type": "boolean"
        },
        "consumes": {
            "enum": [
                "multipart/form-data"
            ]
        }
    }
}