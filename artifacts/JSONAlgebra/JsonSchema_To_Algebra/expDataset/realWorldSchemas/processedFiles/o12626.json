{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "required": [
        "meta",
        "objects"
    ],
    "properties": {
        "meta": {
            "type": "object",
            "properties": {
                "limit": {
                    "type": "integer"
                },
                "next": {
                    "type": "string"
                },
                "offset": {
                    "type": "integer"
                },
                "previous": {
                    "type": [
                        "integer",
                        "null"
                    ]
                },
                "total_count": {
                    "type": "integer"
                }
            }
        },
        "objects": {
            "type": "array"
        }
    }
}
