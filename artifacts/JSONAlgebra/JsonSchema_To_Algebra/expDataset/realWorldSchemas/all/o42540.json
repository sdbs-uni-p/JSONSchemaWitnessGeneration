{
    "id": "http://schemas.gdbots.io/json-schema/gdbots/enrichments/mixin/utm/1-0-0.json#",
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "utm_source": {
            "type": "string",
            "pattern": "^[\\w\\/\\.:-]+$"
        },
        "utm_medium": {
            "type": "string",
            "pattern": "^[\\w\\/\\.:-]+$"
        },
        "utm_term": {
            "type": "string",
            "pattern": "^[\\w\\s\\/\\.,:-]+$"
        },
        "utm_content": {
            "type": "string",
            "pattern": "^[\\w\\/\\.:-]+$"
        },
        "utm_campaign": {
            "type": "string",
            "pattern": "^[\\w\\/\\.:-]+$"
        }
    },
    "additionalProperties": true
}