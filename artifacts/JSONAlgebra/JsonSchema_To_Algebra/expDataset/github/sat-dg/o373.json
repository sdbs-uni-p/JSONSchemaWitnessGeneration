{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "RDAP JSON",
    "description": "JSON representation of rdap entities",
    "type": "object",
    "anyOf": [
        {
            "$ref": "#/definitions/autnum"
        },
        {
            "$ref": "#/definitions/domain"
        },
        {
            "$ref": "#/definitions/entity"
        },
        {
            "$ref": "#/definitions/ip"
        },
        {
            "$ref": "#/definitions/error"
        }
    ],
    "definitions": {
        "autnum": {
            "type": "object",
            "required": [
                "rdapConformance",
                "handle",
                "startAutnum",
                "endAutnum"
            ],
            "properties": {
                "rdapConformance": {
                    "$ref": "#/definitions/rdapConformance"
                },
                "notices": {
                    "$ref": "#/definitions/notices"
                },
                "remarks": {
                    "$ref": "#/definitions/remarks"
                },
                "events": {
                    "$ref": "#/definitions/events"
                },
                "port43": {
                    "type": "string"
                },
                "links": {
                    "$ref": "#/definitions/links"
                },
                "handle": {
                    "type": "string"
                },
                "startAutnum": {
                    "type": "integer"
                },
                "endAutnum": {
                    "type": "integer"
                },
                "name": {
                    "type": "string"
                },
                "country": {
                    "type": "string"
                },
                "entities": {
                    "$ref": "#/definitions/entities"
                }
            },
            "additionalProperties": false
        },
        "domain": {
            "required": [
                "rdapConformance",
                "handle",
                "ldhName",
                "nameservers"
            ],
            "properties": {
                "rdapConformance": {
                    "$ref": "#/definitions/rdapConformance"
                },
                "notices": {
                    "$ref": "#/definitions/notices"
                },
                "remarks": {
                    "$ref": "#/definitions/remarks"
                },
                "events": {
                    "$ref": "#/definitions/events"
                },
                "port43": {
                    "type": "string"
                },
                "links": {
                    "$ref": "#/definitions/links"
                },
                "handle": {
                    "type": "string"
                },
                "ldhName": {
                    "type": "string"
                },
                "nameservers": {
                    "$ref": "#/definitions/nameservers"
                },
                "secureDNS": {
                    "$ref": "#/definitions/secureDNS"
                },
                "entities": {
                    "$ref": "#/definitions/entities"
                },
                "vcardArray": {
                    "$ref": "#/definitions/vcardArray"
                }
            },
            "additionalProperties": false
        },
        "entity": {
            "type": "object",
            "required": [
                "rdapConformance",
                "handle"
            ],
            "properties": {
                "rdapConformance": {
                    "$ref": "#/definitions/rdapConformance"
                },
                "notices": {
                    "$ref": "#/definitions/notices"
                },
                "remarks": {
                    "$ref": "#/definitions/remarks"
                },
                "events": {
                    "$ref": "#/definitions/events"
                },
                "port43": {
                    "type": "string"
                },
                "links": {
                    "$ref": "#/definitions/links"
                },
                "handle": {
                    "type": "string"
                },
                "entities": {
                    "$ref": "#/definitions/entities"
                },
                "vcardArray": {
                    "$ref": "#/definitions/vcardArray"
                }
            },
            "additionalProperties": false
        },
        "ip": {
            "type": "object",
            "required": [
                "rdapConformance",
                "handle",
                "startAddress",
                "endAddress",
                "ipVersion"
            ],
            "properties": {
                "rdapConformance": {
                    "$ref": "#/definitions/rdapConformance"
                },
                "notices": {
                    "$ref": "#/definitions/notices"
                },
                "remarks": {
                    "$ref": "#/definitions/remarks"
                },
                "events": {
                    "$ref": "#/definitions/events"
                },
                "port43": {
                    "type": "string"
                },
                "links": {
                    "$ref": "#/definitions/links"
                },
                "handle": {
                    "type": "string"
                },
                "startAddress": {
                    "type": "string"
                },
                "endAddress": {
                    "type": "string"
                },
                "ipVersion": {
                    "enum": [
                        "v4",
                        "v6"
                    ]
                },
                "name": {
                    "type": "string"
                },
                "type": {
                    "type": "string"
                },
                "country": {
                    "type": "string"
                },
                "parentHandle": {
                    "type": "string"
                },
                "entities": {
                    "$ref": "#/definitions/entities"
                }
            },
            "additionalProperties": false
        },
        "error": {
            "type": "object",
            "required": [
                "rdapConformance",
                "errorCode"
            ],
            "properties": {
                "rdapConformance": {
                    "$ref": "#/definitions/rdapConformance"
                },
                "notices": {
                    "$ref": "#/definitions/notices"
                },
                "remarks": {
                    "$ref": "#/definitions/remarks"
                },
                "events": {
                    "$ref": "#/definitions/events"
                },
                "port43": {
                    "type": "string"
                },
                "links": {
                    "$ref": "#/definitions/links"
                },
                "errorCode": {
                    "type": "integer"
                },
                "title": {
                    "type": "string"
                },
                "description": {
                    "type": "array"
                }
            },
            "additionalProperties": false
        },
        "rdapConformance": {
            "type": "array",
            "minItems": 1,
            "items": {
                "type": "string",
                "enum": [
                    "rdap_level_0"
                ]
            },
            "_uniqueItems": true
        },
        "notices": {
            "type": "array",
            "minItems": 1,
            "items": {
                "type": "object",
                "properties": {
                    "title": {
                        "type": "string"
                    },
                    "description": {
                        "type": "array"
                    },
                    "links": {
                        "$ref": "#/definitions/links"
                    }
                },
                "additionalProperties": false
            }
        },
        "links": {
            "type": "array",
            "minItems": 1,
            "items": {
                "type": "object",
                "properties": {
                    "hreflang": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    },
                    "title": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    },
                    "value": {
                        "format": "uri",
                        "type": "string"
                    },
                    "rel": {
                        "type": "string"
                    },
                    "href": {
                        "format": "uri",
                        "type": "string"
                    },
                    "type": {
                        "type": "string"
                    },
                    "media": {
                        "type": "string"
                    }
                },
                "additionalProperties": false
            }
        },
        "remarks": {
            "type": "array",
            "minItems": 1,
            "items": {
                "type": "object",
                "properties": {
                    "title": {
                        "type": "string"
                    },
                    "description": {
                        "type": "array",
                        "items": {
                            "type": "string"
                        }
                    },
                    "links": {
                        "$ref": "#/definitions/links"
                    }
                },
                "additionalProperties": false
            }
        },
        "events": {
            "type": "array",
            "minItems": 1,
            "items": {
                "type": "object",
                "properties": {
                    "eventAction": {
                        "type": "string"
                    },
                    "eventActor": {
                        "type": "string"
                    },
                    "eventDate": {
                        "format": "date-time"
                    }
                },
                "additionalProperties": false
            }
        },
        "entities": {
            "type": "array",
            "minItems": 1,
            "items": {
                "type": "object",
                "required": [
                    "handle"
                ],
                "properties": {
                    "handle": {
                        "type": "string"
                    },
                    "vcardArray": {
                        "$ref": "#/definitions/vcardArray"
                    },
                    "roles": {
                        "$ref": "#/definitions/roles"
                    },
                    "remarks": {
                        "$ref": "#/definitions/remarks"
                    },
                    "links": {
                        "$ref": "#/definitions/links"
                    }
                }
            }
        },
        "secureDNS": {
            "type": "object",
            "properties": {
                "delegationSigned": {
                    "type": "boolean"
                },
                "zoneSigned": {
                    "type": "boolean"
                },
                "maxSigLife": {
                    "type": "number"
                },
                "dsData": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "required": [
                            "keyTag",
                            "algorithm",
                            "digest",
                            "digestType"
                        ],
                        "minItems": 1,
                        "properties": {
                            "keyTag": {
                                "type": "integer"
                            },
                            "algorithm": {
                                "type": "integer",
                                "minimum": 0,
                                "maximum": 255
                            },
                            "digest": {
                                "type": "string"
                            },
                            "digestType": {
                                "type": "integer"
                            }
                        },
                        "additionalProperties": false
                    }
                }
            }
        },
        "nameservers": {
            "type": "array",
            "minItems": 1,
            "items": {
                "type": "object",
                "properties": {
                    "ldhName": {
                        "type": "string"
                    }
                },
                "additionalProperties": false
            }
        },
        "roles": {
            "type": "array",
            "minItems": 1,
            "items": {
                "type": "string",
                "enum": [
                    "technical",
                    "administrative",
                    "registrant",
                    "abuse",
                    "billing",
                    "registrar",
                    "reseller",
                    "sponsor",
                    "proxy"
                ]
            },
            "_uniqueItems": true
        },
        "vcardArray": {
            "type": "array",
            "items": [
                {
                    "type": "string"
                },
                {
                    "items": {
                        "$ref": "#/definitions/vcard"
                    }
                }
            ]
        },
        "vcard": {
            "type": "array",
            "items": [
                {
                    "type": "string",
                    "enum": [
                        "version",
                        "source",
                        "kind",
                        "fn",
                        "n",
                        "nickname",
                        "photo",
                        "bday",
                        "anniversary",
                        "gender",
                        "adr",
                        "tel",
                        "email",
                        "impp",
                        "lang",
                        "tz",
                        "geo",
                        "title",
                        "role",
                        "logo",
                        "org",
                        "member",
                        "related",
                        "categories",
                        "note",
                        "prodid",
                        "rev",
                        "sound",
                        "uid",
                        "clientpidmap",
                        "url",
                        "key",
                        "fburl",
                        "caladruri",
                        "caluri",
                        "xml"
                    ]
                },
                {
                    "type": "object"
                },
                {
                    "enum": [
                        "text",
                        "uri",
                        "date",
                        "time",
                        "date-time",
                        "boolean",
                        "integer",
                        "float",
                        "utc-offset",
                        "language-tag"
                    ]
                },
                {
                    "type": [
                        "string",
                        "array"
                    ]
                },
                {
                    "type": [
                        "string",
                        "array"
                    ]
                }
            ]
        }
    }
}