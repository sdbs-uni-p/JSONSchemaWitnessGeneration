{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "vorgaenger": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "warteart": {
            "type": "string",
            "enum": [
                "beginn",
                "ende"
            ],
            "default": "beginn"
        },
        "rechner": {
            "type": "string"
        },
        "wartezeit": {
            "type": "string"
        }
    },
    "required": [
        "vorgaenger"
    ]
}