{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "rootArray": {
            "type": "array",
            "items": {
                "type": "array",
                "items": {
                    "type": "object",
                    "properties": {
                        "someString": {
                            "type": "string"
                        },
                        "anotherString": {
                            "type": "string"
                        },
                        "someArray": {
                            "type": "array",
                            "items": {
                                "type": "string"
                            }
                        }
                    },
                    "additionalProperties": false,
                    "required": [
                        "someString",
                        "anotherString",
                        "someArray"
                    ]
                }
            }
        }
    },
    "additionalProperties": false,
    "required": [
        "rootArray"
    ]
}