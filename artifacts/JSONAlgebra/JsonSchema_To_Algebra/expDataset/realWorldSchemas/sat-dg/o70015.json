    "properties": {
        "regex": {
            "description": "Whether to understand the annotation names as regular expressions.",
            "type": "boolean",
            "default": "false"
        },
        "include": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "exclude": {
            "type": "array",
            "items": {
                "type": "string"
            }
        }
    }
}