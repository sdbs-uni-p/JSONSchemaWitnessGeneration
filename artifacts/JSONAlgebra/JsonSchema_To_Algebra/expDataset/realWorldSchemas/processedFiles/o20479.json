{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "additionalProperties": false,
    "id": "http://localhost:3000/schemas/refresh-token-response.json#",
    "properties": {
        "access_token": {
            "type": "string"
        },
        "expires_in": {
            "type": "number"
        },
        "refresh_token": {
            "type": "string"
        },
        "token_type": {
            "enum": [
                "bearer"
            ],
            "type": "string"
        }
    },
    "required": [
        "token_type",
        "access_token",
        "expires_in",
        "refresh_token"
    ],
    "title": "refresh",
    "type": "object"
}