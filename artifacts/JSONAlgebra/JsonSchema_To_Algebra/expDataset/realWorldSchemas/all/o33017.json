{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Problem set",
    "type": "array",
    "items": {
        "title": "Problem",
        "type": "object",
        "properties": {
            "GoalTypes": {
                "type": "array",
                "items": {
                    "type": "string"
                },
                "minItems": 1,
                "_uniqueItems": true
            },
            "SuggestFuzzyTypes": {
                "type": "boolean"
            },
            "SuggestTypes": {
                "type": "array",
                "items": {
                    "type": "string"
                },
                "minItems": 1,
                "_uniqueItems": true
            },
            "Type": {
                "type": "string"
            }
        }
    },
    "required": [
        "Type",
        "GoalTypes",
        "SuggestTypes",
        "SuggestEnvironmentTypes"
    ]
}