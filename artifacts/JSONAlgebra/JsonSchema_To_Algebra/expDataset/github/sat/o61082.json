{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "carrier": {
            "type": "string",
            "enum": [
                "dhl",
                "ups",
                "dpd",
                "hermes",
                "gls"
            ],
            "description": "acronym of the carrier you want to use"
        },
        "width": {
            "type": "number"
        },
        "height": {
            "type": "number"
        },
        "length": {
            "type": "number"
        },
        "weight": {
            "type": "number"
        }
    },
    "required": [
        "carrier",
        "width",
        "height",
        "length",
        "weight"
    ],
    "additionalProperties": false
}