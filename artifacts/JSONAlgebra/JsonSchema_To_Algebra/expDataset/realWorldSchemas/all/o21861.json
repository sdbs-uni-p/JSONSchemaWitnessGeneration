{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "required": [
        "TableName",
        "StreamViewType"
    ],
    "additionalProperties": false,
    "properties": {
        "TableName": {
            "type": "string"
        },
        "StreamViewType": {
            "type": "string",
            "enum": [
                "NEW_IMAGE",
                "OLD_IMAGE",
                "NEW_AND_OLD_IMAGES",
                "KEYS_ONLY"
            ]
        }
    }
}