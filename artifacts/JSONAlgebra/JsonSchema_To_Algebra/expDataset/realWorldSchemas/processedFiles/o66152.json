            },
            "met_type": {
                "enum": [
                    "PFMET",
                    "CaloMET"
                ],
                "type": "string",
                "title": "MET type"
            },
            "track_type": {
                "enum": [
                    "Heavy-Ion",
                    "pp"
                ],
                "type": "string",
                "title": "Track type"
            },
            "object": {
                "enum": [
                    "",
                    "electron",
                    "muon",
                    "tau",
                    "jet",
                    "bjet",
                    "photon",
                    "track",
                    "vertex",
                    "MET",
                    "HT"
                ],
                "type": "string",
                "title": "Object"
            },
            "number": {
                "type": "object",
                "properties": {
                    "number": {
                        "type": "number",
                        "title": "Number"
                    },
                    "sign": {
                        "default": "=",
                        "enum": [
                            "<",
                            ">",
                            "=",
                            "<=",
                            ">="
                        ],
                        "type": "string",
                        "title": "<, >, =, <=, >="
                    }
                },
                "title": ""
            },
            "eta_cut": {
                "type": "number",
                "title": "eta cut"
            },
            "pt_cut_max": {
                "type": "number",
                "title": "pt cut max"
            },
            "electron_type": {
                "enum": [
                    "GsfElectron"
                ],
                "type": "string",
                "title": "Electron type"
            },
            "eta_cut_max": {
                "type": "number",
                "title": "eta cut max"
            },
            "tau_type": {
                "enum": [
                    "PFTau"
                ],
                "type": "string",
                "title": "Tau type"
            },
            "eta_cut_min": {
                "type": "number",
                "title": "eta cut min"
            },
            "pt_cut_min": {
                "type": "number",
                "title": "pt cut min"
            }
