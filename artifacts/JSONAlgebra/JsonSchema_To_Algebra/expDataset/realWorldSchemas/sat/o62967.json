{
    "title": "Proceedings",
    "$schema": "http://json-schema.org/draft-04/schema#",
    "description": "Proceedings represent a queried subsets of all proceedings.",
    "required": [
        "_metadata",
        "proceedings"
    ],
    "type": "object",
    "properties": {
        "_metadata": {
            "required": [
                "request",
                "response"
            ],
            "type": "object",
            "properties": {
                "request": {
                    "required": [
                        "ip",
                        "params",
                        "time",
                        "user"
                    ],
                    "type": "object",
                    "properties": {
                        "ip": {
                            "type": "string"
                        },
                        "params": {
                            "type": "null"
                        },
                        "user": {
                            "type": "null"
                        },
                        "time": {
                            "type": "string"
                        }
                    }
                },
                "response": {
                    "required": [
                        "pagination",
                        "time"
                    ],
                    "type": "object",
                    "properties": {
                        "pagination": {
                            "required": [
                                "limit",
                                "offset",
                                "total"
                            ],
                            "type": "object",
                            "properties": {
                                "limit": {
                                    "type": "integer"
                                },
                                "offset": {
                                    "type": "integer"
                                },
                                "total": {
                                    "type": "integer"
                                }
                            }
                        },
                        "time": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "proceedings": {
            "items": [
                {
                    "required": [
                        "appeal",
                        "applicant_name",
                        "bureau",
                        "bureau_id_num",
                        "callsign",
                        "channel",
                        "city",
                        "consolidated_proceeding_id",
                        "date_archived",
                        "date_closed",
                        "date_commission_decision",
                        "date_designated",
                        "date_effective",
                        "date_initial_decision",
                        "date_nprm",
                        "date_oral_argument",
                        "date_proceeding_created",
                        "date_public_notice",
                        "date_reporting_and_order",
                        "date_rule_board_decision",
                        "exparte_late_filed",
                        "face_card_id",
                        "file_number",
                        "filed_by",
                        "flag_archived",
                        "flag_historical_data_exists",
                        "flag_internet_file",
                        "flag_open_close",
                        "flag_rulemaking_or_docket",
                        "flag_small_business_impact",
                        "id",
                        "id_bureau",
                        "id_state",
                        "location",
                        "proceeding_description",
                        "proceeding_name",
                        "recent_filing_count",
                        "rule_section",
                        "subject",
                        "total_filing_count"
                    ],
                    "type": "object",
                    "properties": {
                        "flag_historical_data_exists": {
                            "type": "string"
                        },
                        "date_closed": {
                            "type": "string"
                        },
                        "file_number": {
                            "type": "string"
                        },
                        "date_initial_decision": {
                            "type": "string"
                        },
                        "date_reporting_and_order": {
                            "type": "string"
                        },
                        "flag_small_business_impact": {
                            "type": "string"
                        },
                        "date_oral_argument": {
                            "type": "string"
                        },
                        "callsign": {
                            "type": "string"
                        },
                        "recent_filing_count": {
                            "type": "integer"
                        },
                        "id": {
                            "type": "integer"
                        },
                        "exparte_late_filed": {
                            "type": "string"
                        },
                        "city": {
                            "type": "string"
                        },
                        "consolidated_proceeding_id": {
                            "type": "integer"
                        },
                        "id_bureau": {
                            "type": "integer"
                        },
                        "bureau": {
                            "required": [
                                "bureau_code",
                                "bureau_name",
                                "edocs_bureau_code",
                                "id"
                            ],
                            "type": "object",
                            "properties": {
                                "bureau_name": {
                                    "type": "string"
                                },
                                "id": {
                                    "type": "integer"
                                },
                                "edocs_bureau_code": {
                                    "type": "null"
                                },
                                "bureau_code": {
                                    "type": "string"
                                }
                            }
                        },
                        "location": {
                            "type": "string"
                        },
                        "date_proceeding_created": {
                            "type": "string"
                        },
                        "total_filing_count": {
                            "type": "integer"
                        },
                        "filed_by": {
                            "type": "string"
                        },
                        "appeal": {
                            "type": "string"
                        },
                        "rule_section": {
                            "type": "string"
                        },
                        "flag_internet_file": {
                            "type": "string"
                        },
                        "date_nprm": {
                            "type": "null"
                        },
                        "subject": {
                            "type": "string"
                        },
                        "flag_archived": {
                            "type": "string"
                        },
                        "flag_open_close": {
                            "type": "string"
                        },
                        "flag_rulemaking_or_docket": {
                            "type": "string"
                        },
                        "id_state": {
                            "type": "integer"
                        },
                        "date_effective": {
                            "type": "string"
                        },
                        "date_public_notice": {
                            "type": "string"
                        },
                        "proceeding_description": {
                            "type": "string"
                        },
                        "applicant_name": {
                            "type": "string"
                        },
                        "date_archived": {
                            "type": "string"
                        },
                        "date_rule_board_decision": {
                            "type": "string"
                        },
                        "date_designated": {
                            "type": "string"
                        },
                        "bureau_id_num": {
                            "type": "string"
                        },
                        "channel": {
                            "type": "string"
                        },
                        "face_card_id": {
                            "type": "null"
                        },
                        "proceeding_name": {
                            "type": "string"
                        },
                        "date_commission_decision": {
                            "type": "string"
                        }
                    }
                }
            ],
            "type": "array"
        }
    }
}