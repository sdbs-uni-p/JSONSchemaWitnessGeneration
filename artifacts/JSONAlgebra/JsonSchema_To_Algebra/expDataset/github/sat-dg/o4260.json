{
    "type": "array",
    "$schema": "http://json-schema.org/draft-04/schema#",
    "description": "",
    "minItems": 1,
    "_uniqueItems": true,
    "items": {
        "type": "object",
        "required": [
            "name",
            "description",
            "supportedGameModes",
            "imageUrl",
            "id",
            "contentId"
        ],
        "properties": {
            "name": {
                "type": "string",
                "minLength": 1
            },
            "description": {
                "type": [
                    "string",
                    "null"
                ],
                "minLength": 1
            },
            "supportedGameModes": {
                "type": "array",
                "minItems": 1,
                "items": {
                    "type": "string",
                    "minLength": 1
                }
            },
            "imageUrl": {
                "type": "string",
                "minLength": 1
            },
            "id": {
                "type": "string",
                "minLength": 1
            },
            "contentId": {
                "type": "string",
                "minLength": 1
            }
        }
    }
}