{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "description": "Fibaro Home Center Weather JSON Schema",
    "name": "weather",
    "type": "object",
    "properties": {
        "ConditionCode": {
            "type": "string"
        },
        "Humidity": {
            "type": "string"
        },
        "PreviousConditionCode": {
            "type": "string"
        },
        "PreviousHumidity": {
            "type": "string"
        },
        "PreviousTemperature": {
            "type": "string"
        },
        "PreviousWeatherConditionConverted": {
            "type": "string"
        },
        "PreviousWind": {
            "type": "string"
        },
        "Temperature": {
            "type": "string"
        },
        "WeatherCondition": {
            "type": "string"
        },
        "WeatherConditionConverted": {
            "type": "string"
        },
        "Wind": {
            "type": "string"
        },
        "saveLogs": {
            "enum": [
                "0",
                "1"
            ]
        },
        "TemperatureUnit": {
            "enum": [
                "C",
                "F"
            ]
        }
    },
    "required": [
        "ConditionCode",
        "Humidity",
        "PreviousConditionCode",
        "PreviousHumidity",
        "PreviousTemperature",
        "PreviousWeatherConditionConverted",
        "PreviousWind",
        "Temperature",
        "WeatherCondition",
        "WeatherConditionConverted",
        "Wind",
        "saveLogs",
        "TemperatureUnit"
    ],
    "additionalProperties": false
}