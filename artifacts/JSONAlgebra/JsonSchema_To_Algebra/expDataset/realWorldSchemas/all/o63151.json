{
    "$schema": "http://json-schema.org/draft-04/schema",
    "title": "Namespace Value Resource",
    "description": "A single namespace value within a BEL Namespace.",
    "type": "object",
    "additionalProperties": false,
    "required": [
        "namespace_value"
    ],
    "properties": {
        "namespace_value": {
            "type": "object",
            "additionalProperties": false,
            "required": [
                "type",
                "identifier",
                "name",
                "title",
                "species"
            ],
            "properties": {
                "type": {
                    "type": "string"
                },
                "identifier": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "title": {
                    "type": [
                        "null",
                        "string"
                    ]
                },
                "species": {
                    "type": [
                        "null",
                        "string"
                    ]
                },
                "match_text": {
                    "type": "string"
                }
            }
        }
    }
}