{
    "$schema": "http://json-schema.org/draft-04/schema#4",
    "title": "IdLIst",
    "description": "Contains multiple elements of unique identifiers",
    "type": "object",
    "properties": {
        "idType": {
            "enum": [
                "stations",
                "pumps",
                "tanks"
            ]
        },
        "data": {
            "description": "array of ids",
            "type": "array",
            "items": {
                "type": "string"
            }
        }
    },
    "required": [
        "idType",
        "data"
    ]
}