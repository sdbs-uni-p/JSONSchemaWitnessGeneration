{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "description": "Edges in a Guice injector graph.",
    "type": "object",
    "required": [
        "fromId",
        "toId",
        "type"
    ],
    "properties": {
        "fromId": {
            "type": "string"
        },
        "toId": {
            "type": "string"
        },
        "type": {
            "enum": [
                "binding",
                "dependency"
            ]
        },
        "bindingType": {
            "type": "string"
        }
    }
}