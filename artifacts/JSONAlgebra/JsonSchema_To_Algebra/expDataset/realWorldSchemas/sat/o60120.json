{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "description": "A document returned by GET /{db}/_design/{ddoc}",
    "properties": {
        "_id": {
            "type": "string"
        },
        "_rev": {
            "type": "string"
        },
        "language": {
            "type": "string"
        },
        "options": {
            "type": "object"
        },
        "filters": {
            "type": "object"
        },
        "lists": {
            "type": "object"
        },
        "rewrites": {
            "items": {
                "type": "object"
            },
            "type": "array"
        },
        "shows": {
            "type": "object"
        },
        "updates": {
            "type": "object"
        },
        "validate_doc_update": {
            "type": "string"
        },
        "views": {
            "type": "object"
        }
    },
    "required": [
        "_id",
        "_rev"
    ],
    "title": "CouchDB GET of design document",
    "type": "object"
}