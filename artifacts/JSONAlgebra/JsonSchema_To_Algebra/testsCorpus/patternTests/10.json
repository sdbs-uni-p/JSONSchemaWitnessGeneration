{
  "type": "object",
  "required": [
    "DestinationCidrBlock",
    "RouteTableId",
    "NatGatewayId"
  ],
  "properties": {
    "DestinationCidrBlock": {
      "type": "string",
      "pattern": "^(([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])\\.){3}([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])(/(\\d|[1-2]\\d|3[0-2]))|0\\.0\\.0\\.0\\/0$"
    },
    "RouteTableId": {
      "type": "string",
      "pattern": "^[a-z][a-z0-9\\-]+$"
    },
    "NatGatewayId": {
      "type": "string",
      "pattern": "^[a-z][a-z0-9\\-]+$"
    }
  }
}